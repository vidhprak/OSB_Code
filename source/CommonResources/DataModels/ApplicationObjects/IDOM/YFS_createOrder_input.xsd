    <xsd:element name="OrderLine">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE">
            <xsd:documentation>
                <div>
					Optional. Stores order line information, including item details and shipping information. Multiple <code>OrderLine</code>s can be created for a single order. At least one <code>OrderLine</code> must exist for an order if order is getting created with <code>DraftOrderFlag</code> as 'N'. For draft order, line must be added before confirming the order.
						<Validation>
                        <Condition>No <code>OrderLine</code> passed.</Condition>
                        <Error Description="No Line." ErrorCode="YFS10015"/>
                    </Validation>
                    <Validation>
                        <Condition>Line is a bundle parent and child lines are automatically created based on the components of the bundle in the catalog, which contains a model item.</Condition>
                        <Error Description="Cannot include a bundle item with a model item as a component." ErrorCode="OMP80_0040"/>
                    </Validation>
                </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrderLineTranQuantity"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:DerivedFrom"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ChainedFrom"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrderLineOptions"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrderLineSourcingControls"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:Item"/>
			<!-- The element Order/OrderLines/OrderLine/Extn starts.... -->
				<xsd:element maxOccurs="1" minOccurs="0" name="Extn">
					 <xsd:complexType>
                        <xsd:attribute name="ExtnGroup" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>Group Name.</xsd:documentation>
                            </xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
