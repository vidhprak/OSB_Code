                        <xsd:attribute name="MobilePhone" type="yfctype:PhoneNo" use="optional">
                            <xsd:annotation yfc:columnName="MOBILE_PHONE" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The mobile phone at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OtherPhone" type="yfctype:PhoneNo" use="optional">
                            <xsd:annotation yfc:columnName="OTHER_PHONE" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The alternate phone number at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PersonID" type="yfctype:PersonId" use="optional">
                            <xsd:annotation yfc:columnName="PERSON_ID" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The unique identifier for a person.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="State" type="yfctype:State" use="optional">
                            <xsd:annotation yfc:columnName="STATE" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The state in the address.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Suffix" type="yfctype:Suffix" use="optional">
                            <xsd:annotation yfc:columnName="SUFFIX" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The suffix for a person, such as Jr., or Sr.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TaxGeoCode" type="yfctype:GeoCode" use="optional">
                            <xsd:annotation yfc:columnName="TAX_GEO_CODE" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The tax geo code for this location. Intended for future use.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Title" type="yfctype:Title-10" use="optional">
                            <xsd:annotation yfc:columnName="TITLE" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The title for the person such as Ms., Mr., or Dr..</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ZipCode" type="yfctype:ZipCode" use="optional">
                            <xsd:annotation yfc:columnName="ZIP_CODE" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The zip code in the address.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="Instructions">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded" minOccurs="0" name="Instruction">
                                <xsd:annotation yfc:tableName="YFS_INSTRUCTION_DETAIL">
                                    <xsd:documentation>Generic table used to store instructions associated with various entities in the system.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute name="InstructionText" type="yfctype:InstructionText" use="required">
                                    <xsd:annotation yfc:columnName="INSTRUCTION_TEXT" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                                    <xsd:documentation>
                                    <div>
				Free-form text indicating any additional information related to the instruction type. For example, special
				gift wrapping instructions.
				<Validation>
                                    <Condition>If <code>Instruction</code> is passed, then <code>InstructionText</code> must be passed.
					</Condition>
                                    <Error Description="Blank Instruction Text." ErrorCode="YFS10386"/>
                                    </Validation>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="InstructionType" type="yfctype:InstructionType" use="optional">
                                    <xsd:annotation yfc:columnName="INSTRUCTION_TYPE" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                                    <xsd:documentation>Categorization of instructions so that they can be grouped together for reference. For example, you may have instructions of type Pick, Pack, and Ship for an order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="InstructionURL" type="yfctype:URL" use="optional">
                                    <xsd:annotation yfc:columnName="INSTRUCTION_URL" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                                    <xsd:documentation>The URL relating to and/or containing the instructions.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="SequenceNo" type="yfctype:Count" use="optional">
                                    <xsd:annotation yfc:columnName="SEQUENCE_NO" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                                    <xsd:documentation>You could have an instruction exceeding the limits of a single record. Sequence number is used to provide a way to store a link between such instructions.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:LinePriceInfo"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:LineCharges"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:LineTaxes"/>
                <xsd:element maxOccurs="1" minOccurs="0" name="References">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded" minOccurs="0" name="Reference">
                                <xsd:annotation yfc:tableName="YFS_REFERENCE_TABLE">
                                    <xsd:documentation>This table stores dynamic order references that can be passed to the createOrder API and returned by the getOrderDetails API. Database extensibility of the order tables should be used instead of this table to store implementation specific order fields.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute name="Name" type="yfctype:Text-40" use="required">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
							Name of the reference. Required if the <code>Reference</code> element is passed. The <code>Name</code> can be any text, with no white spaces, within, following or before the text.
							<Validation>
                                    <Condition>
                                    <code>Name</code> not passed.
								</Condition>
                                    <Error Description="Blank Reference Name" ErrorCode="YFS103876"/>
                                    </Validation>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Value" type="yfctype:Text-40" use="optional"/>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:KitLines"/>
                <xsd:element maxOccurs="1" minOccurs="0" name="AdditionalAddresses">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded" minOccurs="0" name="AdditionalAddress">
                                <xsd:annotation yfc:tableName="YFS_ENTITY_ADDRESS">
                                    <xsd:documentation>
                                    <div>
				Multiple addresses can be associated with an <code>Order</code> and for individual <code>OrderLines</code> if the <code>AdditionalAddress</code> entity is passed in the inputXML. The system does not have any application logic based on <code>AdditionalAddress</code>.
				</div>
                                    </xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:all>
                                    <xsd:element maxOccurs="1" minOccurs="0" name="PersonInfo">
                                    <xsd:annotation yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>This table stores relevant personal information and addresses. Addresses are typically organization address, ship node address, customer address, and so forth. Personal information is required in cases where person to person contact needs to be established; for example, taking appointments or speaking during negotiations. The key of this table is posted in various tables to allow access to people and address information.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                    <xsd:attribute name="AddressID" type="yfctype:Text-256" use="optional">
                                    <xsd:annotation yfc:columnName="ADDRESS_ID" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>A user specified label for an address.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AddressLine1" type="yfctype:Address" use="optional">
                                    <xsd:annotation yfc:columnName="ADDRESS_LINE1" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The address line 1 field.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AddressLine2" type="yfctype:Address" use="optional">
                                    <xsd:annotation yfc:columnName="ADDRESS_LINE2" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The address line 2 field.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AddressLine3" type="yfctype:Address" use="optional">
                                    <xsd:annotation yfc:columnName="ADDRESS_LINE3" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The address line 3 field.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AddressLine4" type="yfctype:Address" use="optional">
                                    <xsd:annotation yfc:columnName="ADDRESS_LINE4" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The address line 4 field.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AddressLine5" type="yfctype:Address" use="optional">
                                    <xsd:annotation yfc:columnName="ADDRESS_LINE5" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The address line 5 field.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AddressLine6" type="yfctype:Address" use="optional">
                                    <xsd:annotation yfc:columnName="ADDRESS_LINE6" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The address line 6 field.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AlternateEmailID" type="yfctype:EMailID" use="optional">
                                    <xsd:annotation yfc:columnName="ALTERNATE_EMAILID" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The alternate email address at which the person or a predesignated employee of the organization referred to in the address can be contacted.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Beeper" type="yfctype:PhoneNo" use="optional">
                                    <xsd:annotation yfc:columnName="BEEPER" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The beeper number at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="City" type="yfctype:City" use="optional">
                                    <xsd:annotation yfc:columnName="CITY" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The city in the address.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Company" type="yfctype:Company" use="optional">
                                    <xsd:annotation yfc:columnName="COMPANY" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The company this person is associated with.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Country" type="yfctype:Country" use="optional">
                                    <xsd:annotation yfc:columnName="COUNTRY" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The country in the address.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="DayFaxNo" type="yfctype:PhoneNo" use="optional">
                                    <xsd:annotation yfc:columnName="DAY_FAX_NO" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The day time fax at which the person or a predesignated employee of the organization referred to in the address can be notified.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="DayPhone" type="yfctype:PhoneNo" use="optional">
                                    <xsd:annotation yfc:columnName="DAY_PHONE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The day time phone at which the person or organization referred to in the address can be called.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Department" type="yfctype:Department" use="optional">
                                    <xsd:annotation yfc:columnName="DEPARTMENT" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The primary department in charge of procurement.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="EMailID" type="yfctype:EMailID" use="optional">
                                    <xsd:annotation yfc:columnName="EMAILID" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The email address at which the person or a predesignated employee of the organization referred to in the address can be contacted.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="EveningFaxNo" type="yfctype:PhoneNo" use="optional">
                                    <xsd:annotation yfc:columnName="EVENING_FAX_NO" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The evening time fax at which the person or a predesignated employee of the organization referred to in the address can be notified.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="EveningPhone" type="yfctype:PhoneNo" use="optional">
                                    <xsd:annotation yfc:columnName="EVENING_PHONE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The evening time phone at which the person or organization referred to in the address can be called.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="FirstName" type="yfctype:PersonName" use="optional">
                                    <xsd:annotation yfc:columnName="FIRST_NAME" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The first name of the person.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="IsCommercialAddress" type="yfctype:Flag" use="optional">
                                    <xsd:annotation yfc:columnName="IS_COMMERCIAL_ADDRESS" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>Indicates whether the address is commercial, residential or none. Values of this field can be Y -Commercial address.N - Residential address. undefined for Null.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="JobTitle" type="yfctype:Title-40" use="optional">
                                    <xsd:annotation yfc:columnName="JOB_TITLE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The job title of the person.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="LastName" type="yfctype:PersonName" use="optional">
                                    <xsd:annotation yfc:columnName="LAST_NAME" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The last name of the person.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="MiddleName" type="yfctype:MiddleName" use="optional">
                                    <xsd:annotation yfc:columnName="MIDDLE_NAME" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The middle name of the person.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="MobilePhone" type="yfctype:PhoneNo" use="optional">
                                    <xsd:annotation yfc:columnName="MOBILE_PHONE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The mobile phone at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="OtherPhone" type="yfctype:PhoneNo" use="optional">
                                    <xsd:annotation yfc:columnName="OTHER_PHONE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The alternate phone number at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="PersonID" type="yfctype:PersonId" use="optional">
                                    <xsd:annotation yfc:columnName="PERSON_ID" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The unique identifier for a person.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="State" type="yfctype:State" use="optional">
                                    <xsd:annotation yfc:columnName="STATE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The state in the address.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Suffix" type="yfctype:Suffix" use="optional">
                                    <xsd:annotation yfc:columnName="SUFFIX" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The suffix for a person, such as Jr., or Sr.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="TaxGeoCode" type="yfctype:GeoCode" use="optional">
                                    <xsd:annotation yfc:columnName="TAX_GEO_CODE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The tax geo code for this location. Intended for future use.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Title" type="yfctype:Title-10" use="optional">
                                    <xsd:annotation yfc:columnName="TITLE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The title for the person such as Ms., Mr., or Dr..</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ZipCode" type="yfctype:ZipCode" use="optional">
                                    <xsd:annotation yfc:columnName="ZIP_CODE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The zip code in the address.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:all>
                                    <xsd:attribute name="AddressType" type="yfctype:AddressType" use="required">
                                    <xsd:annotation yfc:columnName="ADDRESS_TYPE" yfc:tableName="YFS_ENTITY_ADDRESS">
                                    <xsd:documentation>Identifies the type of address. Part of unique key</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:Schedules"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:Dependency"/>
                <xsd:element maxOccurs="1" minOccurs="0" name="OrderDates">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded" minOccurs="0" name="OrderDate">
                                <xsd:annotation yfc:tableName="YFS_ORDER_DATE">
                                    <xsd:documentation>This table stores custom and milestone dates for orders.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute name="ActualDate" type="yfctype:TimeStamp" use="optional">
                                    <xsd:annotation yfc:columnName="ACTUAL_DATE" yfc:tableName="YFS_ORDER_DATE">
                                    <xsd:documentation>Actual Date.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="CommittedDate" type="yfctype:TimeStamp" use="optional">
                                    <xsd:annotation yfc:columnName="COMMITTED_DATE" yfc:tableName="YFS_ORDER_DATE">
                                    <xsd:documentation>It is a data attribute to store committed dates from supplier.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="DateTypeId" type="yfctype:ID-40" use="required">
                                    <xsd:annotation yfc:columnName="DATE_TYPE_ID" yfc:tableName="YFS_ORDER_DATE">
                                    <xsd:documentation>The unique identifier for a date type for which additional dates are stored.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ExpectedDate" type="yfctype:TimeStamp" use="optional">
                                    <xsd:annotation yfc:columnName="EXPECTED_DATE" yfc:tableName="YFS_ORDER_DATE">
                                    <xsd:documentation>Expected Date.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="RequestedDate" type="yfctype:TimeStamp" use="optional">
                                    <xsd:annotation yfc:columnName="REQUESTED_DATE" yfc:tableName="YFS_ORDER_DATE">
                                    <xsd:documentation>Requested Date.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrderLineInvAttRequest"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ProductItems"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrderLineReservations"/>
                <xsd:element maxOccurs="1" minOccurs="0" name="Promotions">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded" minOccurs="0" name="Promotion">
                                <xsd:annotation yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>
                                    <div>
						Promotions that are associated to an order or any of its lines. PromotionId must be passed to identify the promotion.
						</div>
                                    </xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute name="Action" type="yfctype:Action" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
						Action to perform on this promotion.
						<br/>Default: <code>Null</code> - if not passed, promotion will be added if it doesn't exist or modified if it does exist.
						<br/>Valid Values: 
							<br/>
                                    <b>
                                    <code>CREATE</code>
                                    </b> - Add the specified promotion to the order.
							<br/>
                                    <b>
                                    <code>REMOVE</code>
                                    </b> - Removes the specified promotion from the order.
						<Validation>
                                    <Condition>If an award record is associated with this promotion, it may not be removed.</Condition>
                                    <Error Description="You are attempting to delete a Promotion associated with an award on the order." ErrorCode="OMP77_10004"/>
                                    </Validation>
                                    <Validation>
                                    <Condition>If a promotion already exists, can't add the same promotion twice.</Condition>
                                    <Error Description="Record already exists in the database." ErrorCode="YFC0001"/>
                                    </Validation>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="DenialReason" type="yfctype:Text-100" use="optional">
                                    <xsd:annotation yfc:columnName="DENIAL_REASON" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>If the promotion is denied, the reason for the denial.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="OverrideAdjustmentValue" use="optional">
                                    <xsd:annotation yfc:columnName="OVERRIDE_ADJUSTMENT_VALUE" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>For MANUAL promotions, the overridden adjustment value for the manual pricing rule.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:Money"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute name="PromotionApplied" type="yfctype:Flag" use="optional">
                                    <xsd:annotation yfc:columnName="PROMOTION_APPLIED" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>A flag indicating if the promotion is applied.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="PromotionId" type="yfctype:PromoCode" use="optional">
                                    <xsd:annotation yfc:columnName="PROMOTION_ID" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>This identifies the promotion that is added to this order. This will be used as the CouponID when determining the price of the order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="PromotionType" type="yfctype:Text-100" use="optional">
                                    <xsd:annotation yfc:columnName="PROMOTION_TYPE" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>The type of promotion. Like DISCOUNT, BOGO.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                        <xsd:attribute name="Reset" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>Determines whether all Promotions, except those listed below, 
					should be removed.</div>
                                </xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:restriction base="yfctype:Flag">
                                    <xsd:enumeration value="N"/>
                                    <xsd:enumeration value="Y"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="Awards">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded" minOccurs="0" name="Award">
                                <xsd:annotation yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>
                                    <div>
					Awards that are created out of the promotions. A single promotion can result in to multiple awards. AwardId must be passed to identify the award.
			<Validation>
                                    <Condition>PromotionId must be passed and it must be a valid one to add the award</Condition>
                                    <Error Description="All awards must be associated to a promotion on the order" ErrorCode="OMP77_10003"/>
                                    </Validation>
                                    </div>
                                    </xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute name="Action" type="yfctype:Action" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Action to perform on this <code>Award</code>.
					<br/>Default: <code>Null</code> - if not passed awards will be added if not exist or modified.
					<br/>Valid Values: 
						<br/>
                                    <b>
                                    <code>REMOVE</code>
                                    </b> - Removes the award.
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AwardAmount" type="yfctype:Money" use="optional">
                                    <xsd:annotation yfc:columnName="AWARD_AMOUNT" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>The amount of discount or charge that was applied as a part of this award.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AwardApplied" type="yfctype:Flag" use="optional">
                                    <xsd:annotation yfc:columnName="AWARD_APPLIED" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>A flag indicating if the award is applied.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AwardId" type="yfctype:Text-120" use="optional">
                                    <xsd:annotation yfc:columnName="AWARD_ID" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>A unique identifier for the award. If pricing is maintained within the system, this will be the PricingRuleName of the pricing rule that created this award.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AwardType" type="yfctype:Text-100" use="optional">
                                    <xsd:annotation yfc:columnName="AWARD_TYPE" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>The type of award. If pricing is maintained within the system, this will be the RuleCategory of the pricing rule that created this award.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ChargeCategory" type="yfctype:ChargeCategory" use="optional">
                                    <xsd:annotation yfc:columnName="CHARGE_CATEGORY" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>The charge category of the LineCharge or HeaderCharge for which this award was created.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ChargeName" type="yfctype:ChargeName" use="optional">
                                    <xsd:annotation yfc:columnName="CHARGE_NAME" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>The charge name of the LineCharge or HeaderCharge for which this award was created.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="DenialReason" type="yfctype:Text-100" use="optional">
                                    <xsd:annotation yfc:columnName="DENIAL_REASON" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>If the award is denied, the reason for the denial.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Description" type="yfctype:Text-2000" use="optional">
                                    <xsd:annotation yfc:columnName="DESCRIPTION" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>A description for the award. If pricing is maintained within the system, this will be the Description of the pricing rule that created this award.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="PosReasonCode" type="yfctype:Text-100" use="optional">
                                    <xsd:annotation yfc:columnName="POS_REASON_CODE" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>Reason this promotion was applied.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="PromotionId" type="yfctype:PromoCode" use="optional">
                                    <xsd:annotation yfc:columnName="PROMOTION_ID" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>This identifies the promotion that is added to this order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="Notes">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded" minOccurs="0" name="Note">
                                <xsd:annotation yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>Generic table used to stores notes associated with various entities in the system.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute name="AuditTransactionId" type="yfctype:TransactionID" use="optional">
                                    <xsd:annotation yfc:columnName="AUDIT_TRANSACTION_ID" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>The identifier of the order modification audit that caused this record.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ContactReference" type="yfctype:NotesContactReference" use="optional">
                                    <xsd:annotation yfc:columnName="CONTACT_REFERENCE" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>If the notes were entered because of customer contact by phone, the phone no can be stored in this column. If the contact is by email, email ID can be stored here.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ContactTime" type="yfctype:TimeStamp" use="optional">
                                    <xsd:annotation yfc:columnName="CONTACT_TIME" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>The time at which the customer contact occurred.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ContactType" type="yfctype:CommonCode" use="optional">
                                    <xsd:annotation yfc:columnName="CONTACT_TYPE" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>The contact type like Phone call, email etc. The values will be selected from common code of code type "CONTACT_TYPE"</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ContactUser" type="yfctype:UserId" use="optional">
                                    <xsd:annotation yfc:columnName="CONTACT_USER" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>The user who made the contact. The default value will the logged in user.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="CustomerSatIndicator" type="yfctype:CustomerSatisfactionLevel" use="optional">
                                    <xsd:annotation yfc:columnName="CUSTOMER_SAT_INDICATOR" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>The customer satisfaction level.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="NoteText" type="yfctype:NoteText" use="required">
                                    <xsd:annotation yfc:columnName="NOTE_TEXT" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>Detailed Notes.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Priority" type="yfctype:PriorityWithDecimal" use="optional">
                                    <xsd:annotation yfc:columnName="PRIORITY" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>Priority of the note. 1 is High, 0 is Low.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ReasonCode" type="yfctype:ReasonCode" use="optional">
                                    <xsd:annotation yfc:columnName="REASON_CODE" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>The reason for which the notes are logged. Like "CustomerContact" etc. The values will be selected from common code of code type "NOTES_REASON"</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="SequenceNo" type="yfctype:Count" use="optional">
                                    <xsd:annotation yfc:columnName="SEQUENCE_NO" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>The sequence of the note.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="VisibleToAll" type="yfctype:Flag" use="optional">
                                    <xsd:annotation yfc:columnName="VISIBLE_TO_ALL" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>This flag determines whether the note applied should be visible to all users. This field is used for filtering. No logic is currently applied to it.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:BundleParentLine"/>
                <xsd:element maxOccurs="1" minOccurs="0" name="OrderHoldTypes">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded" minOccurs="0" name="OrderHoldType">
                                <xsd:annotation yfc:tableName="YFS_ORDER_HOLD_TYPE">
                                    <xsd:documentation>
                                    <div>
			Optional. Stores hold type information. Multiple <code>HoldType</code>s can be passed for a single order. These hold types will be applied in addition to hold types configured to be applied during create order.
			</div>
                                    </xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute name="HoldType" type="yfctype:CodeType" use="required">
                                    <xsd:annotation yfc:columnName="HOLD_TYPE" yfc:tableName="YFS_ORDER_HOLD_TYPE">
                                    <xsd:documentation>
                                    <div>
					The code identifying the hold type to be applied on Order.
					<Validation>
                                    <Condition>
                                    <code>HoldType</code> must be defined in the configuration
						</Condition>
                                    <Error Description="Hold Type is not setup." ErrorCode="OMP11035"/>
                                    </Validation>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ReasonText" type="yfctype:ReasonText" use="optional">
                                    <xsd:annotation yfc:columnName="REASON_TEXT" yfc:tableName="YFS_ORDER_HOLD_TYPE">
                                    <xsd:documentation>The reason text associates to the order hold type.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ResolverUserId" type="yfctype:LoginId" use="optional">
                                    <xsd:annotation yfc:columnName="RESOLVER_USER_ID" yfc:tableName="YFS_ORDER_HOLD_TYPE">
                                    <xsd:documentation>This is the user who can resolve or reject this hold. This field maps to User_Id field of YFS_Customer_Contact.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrderOverride"/>
                <xsd:element maxOccurs="1" minOccurs="0" name="CustomAttributes">
                    <xsd:annotation yfc:tableName="YFS_ORDER_LINE_EXTENSION">
                        <xsd:documentation>This table stores dynamic extension attributes of order line entity.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType/>
                </xsd:element>
            </xsd:all>
            <xsd:attribute name="AllocationDate" type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="ALLOCATE_DATE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The date when this line will be allocated. This is calculated at order creation time, based on the node processing time. This field is not used from version 50 onwards. The ALLOCATION_LEAD_TIME field is used instead.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="BOMXML" type="yfctype:String" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>The BOM XML to be used to create the order line and its components.
                    	</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CarrierAccountNo" type="yfctype:AccountNo" use="optional">
                <xsd:annotation yfc:columnName="CARRIER_ACCOUNT_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The customers carrier account number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CarrierServiceCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="CARRIER_SERVICE_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The carrier service code for this order line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CommittedQuantity" use="optional">
                <xsd:annotation yfc:columnName="COMMITTED_QUANTITY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>It is a data attribute to store the quantity that is committed by supplier.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Number"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="ConditionVariable1" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="CONDITION_VARIABLE_1" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>A user defined variable that can be used for condition building in process modeling.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ConditionVariable2" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="CONDITION_VARIABLE_2" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>A user defined variable that can be used for condition building in process modeling.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ConfigurationKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="BOM_CONFIG_KEY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This field is used by Sterling Multi-Channel Selling Solution so that Visual Modeler can be opened with existing options.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CustomerLinePONo" type="yfctype:Text-10" use="optional">
                <xsd:annotation yfc:columnName="CUSTOMER_PO_LINE_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The buyer PO line number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CustomerPONo" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="CUSTOMER_PO_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The buyer's PO number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DeliveryCode" type="yfctype:DeliveryCode" use="optional">
                <xsd:annotation yfc:columnName="DELIVERY_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This field indicates who will pay the freight. Application understands values such as SUPPLIER, ENTERPRISE and MARKETPLACE to mean that each of these organizations will pay for the freight. You could however pass any other value which is a valid shipment charge type as understood by the carrier by which the shipment is transported. See YFS_SHIPMENT_CONTAINER.carrier_payment_type for additional information.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DeliveryMethod" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="DELIVERY_METHOD" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
						Describes how product is delivered. <code>DeliveryMethod</code> is only valid for product lines.
						<br/>Valid Values: <b>
                                <code>DEL</code>
                            </b> - Delivery, <b>
                                <code>SHP</code>
                            </b>-Ship or <b>
                                <code>PICK</code>
                            </b>- Pickup by the user.
  					    <br/>If a <code>DeliveryMethod</code> value is not passed, the value is set as follows:
					    <br/>If the <code>ProductLine</code> has Delivery associations, set to <b>
                                <code>DEL</code>
                            </b>.
					    <br/>If the <code>Item</code>'s <code>DefaultDeliveryMethod</code> has a value of <b>
                                <code>Y</code>

                            </b> in the catalog, then the <code>Item</code> is checked for shipping methods, as defined by the following fields in the Catalog:
						 <table border="0" cellpadding="0" cellspacing="0" class="table">
                                <tbody>
                                    <tr>
                                    <td class="tablecolumn">
                                    <b>Condition</b>
                                    </td>
                                    <td class="tablecolumn">DeliveryMethod value</td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn">
                                    <code>IS_SHIPPING_ALLLOWED is <b>
                                    <code>Y</code>
                                    </b>
                                    </code>
                                    </td>
                                    <td class="tablecolumn">
                                    <code>SHP</code>
                                    </td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn">
                                    <code>IS_DELIVERY_ALLLOWED is <b>
                                    <code>Y</code>
                                    </b>
                                    </code>
                                    </td>
                                    <td class="tablecolumn">
                                    <code>DEL</code>
                                    </td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn">
                                    <code>IS_PICKUP_ALLLOWED is <b>
                                    <code>Y</code>
                                    </b>
                                    </code>
                                    </td>
                                    <td class="tablecolumn">
                                    <code>PICK</code>
                                    </td>
                                    </tr>
                                </tbody>
                            </table>
                            <Validation>
                                <Condition>If a value is passed, it must be a valid value.
						   </Condition>
                                <Error Description="Invalid Delivery Method Passed" ErrorCode="OMP10020"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DepartmentCode" type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="DEPARTMENT_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
						This field can be used for line classification and reporting purposes. If department code is passed at order header but not passed at order line then value at order header is pushed to order line.

						 </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DependencyShippingRule" type="yfctype:Text-2" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
							Indicates how the <code>OrderLines</code> are dependent on the parent line. The <code>DependencyShippingRule</code> can only be specified for the parent line.
							<br/> If <code>DependencyShippingRule</code> is specified on the parent line, it is defaulted to the same value on all the dependent lines. If <code>DependencyShippingRule</code> is passed on any of the dependent lines, the value passed is ignored.
							<br/>Default: " "  (Null)
							<br/>Valid values: <b>
                                <code>01</code>
                            </b> - Ship together,<b>
                                <code>02</code>
                            </b> - Deliver together
							<br/>
                            <br/>If <code>DependencyShippingRule</code> value is <b>
                                <code>02</code>
                            </b>, a <code>MergeNode</code> must be specified. Order release created for these lines have a <code>MergeNode</code> value, so shipments are  first made to the <code>MergeNode</code>, where they are consolidated for final shipment.
							<br/>
                            <br/>The <code>DependencyShippingRule</code> can only be specified for a product line (<code>ItemGroupCode</code> value is <b>
                                <code>PROD</code>
                            </b>). A product line associated with a delivery line cannot be a part of any dependency shipping rule.
							 <Validation>
                                <Condition>If <code>DependencyShippingRule</code> is passed for delivery or provided service line or an associated product line.
								 </Condition>
                                <Error Description="A Product Line with Dependency Shipping Rule cannot be associated with Delivery Service Line." ErrorCode="OMP10041"/>
                                <Error Description="Service Line cannot have Dependency Shipping Rule" ErrorCode="OMP10058"/>
                            </Validation>
                            <Validation>
                                <Condition>If <code>MergeNode</code> is not specified for parent line, and  <code>DependencyShippingRule</code> value is <b>
                                    <code>02</code>
                                    </b>.
								 </Condition>
                                <Error Description="Merge node is required for shipping rule deliver together" ErrorCode="YFS10443"/>
                            </Validation>
                            <Validation>
                                <Condition>Invalid value is passed.
								 </Condition>
                                <Error Description="Invalid dependency shipping rule" ErrorCode="YFS10450"/>
                            </Validation>
                            <br/>

                            <br/>Based on the <code>DependencyShippingRule</code>, some of attributes on dependent lines are defaulted from the parent line.  For these attributes have any value other than what parent line has, the value is ignored. In addition to following attributes <code>EarliestShipDate</code> and <code>EarliestDeliveryDate</code> are also copied from parent line to dependent line.
							 <div class="innerpanel" width="30%">
                                <table border="0" cellpadding="0" cellspacing="0" class="table">
                                    <tbody>
                                    <tr>
                                    <td class="tablecolumn">
                                    <b>Value</b>
                                    </td>
                                    <td class="tablecolumn">
                                    <b>ShippingRule</b>
                                    </td>
                                    <td class="tablecolumn">
                                    <b>ShipNode</b>
                                    </td>
                                    <td class="tablecolumn">
                                    <b>ReceivingNode</b>
                                    </td>
                                    <td class="tablecolumn">
                                    <b>ShipTo</b>
                                    </td>
                                    <td class="tablecolumn">
                                    <b>MergeNode</b>
                                    </td>
                                    <td class="tablecolumn">
                                    <b>ReqShipDate</b>
                                    </td>
                                    <td class="tablecolumn">
                                    <b>ReqDeliveryDate</b>
                                    </td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn">01</td>
                                    <td class="tablecolumn">defaults</td>
                                    <td class="tablecolumn">defaults</td>
                                    <td class="tablecolumn">- </td>
                                    <td class="tablecolumn">- </td>
                                    <td class="tablecolumn">-</td>
                                    <td class="tablecolumn">defaults</td>
                                    <td class="tablecolumn">defaults</td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn">02</td>
                                    <td class="tablecolumn">defaults</td>
                                    <td class="tablecolumn">-</td>
                                    <td class="tablecolumn">defaults</td>
                                    <td class="tablecolumn">defaults</td>
                                    <td class="tablecolumn"> defaults</td>
                                    <td class="tablecolumn">defaults</td>
                                    <td class="tablecolumn">defaults</td>
                                    </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DistributionRuleId" type="yfctype:DistributionRuleId" use="optional">
                <xsd:annotation yfc:columnName="DISTRIBUTION_RULE_ID" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
						If passed then supplies and demands are read applying the distribution rule filter for inventory availability.
						<Validation>
                                <Condition>Must be a valid <code>DistributionRuleID</code> for the seller organization passed.
							</Condition>
                                <Error Description="Distribution Rule not set up for seller" ErrorCode="YFS10495"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EarliestScheduleDate" use="optional">
                <xsd:annotation yfc:columnName="EARLIEST_SCHEDULE_DATE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The date before which the order line will not be picked up by scheduling.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:TimeStamp"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="FillQuantity" type="yfctype:Quantity" use="optional">
                <xsd:annotation yfc:columnName="FILL_QUANTITY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>The minimum quantity of the <code>OrderedQty</code> that must be fulfilled in a single release.
							<br/>Default: <b>
                                <code>0</code>
                            </b>
                            <Validation>
                                <Condition>Any positive number not greater than line ordered quantity.
								</Condition>
                                <Error Description="Invalid Fill Quantity." ErrorCode="YFS10498"/>
                            </Validation>
                            <Validation>
                                <Condition>If invalid number is passed
								</Condition>
                                <Error Description="java.lang.NumberFormatException" ErrorCode=""/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FirstIterationSeqNo" use="optional">
                <xsd:annotation yfc:columnName="FIRST_ITER_SEQ_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The first iteration sequence for which this line is applicable. If Null, it is assumed that the line applies to all sequences before LastIterationSeqNo. Default is Null.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:LongSequence"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="FreightTerms" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="FOB" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>Indicates Freight Terms for the line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FulfillmentType" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="FULFILLMENT_TYPE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The method of fulfilling the order line during scheduling. This field can be matched with the corresponding field in the YFS_SOURCING_RULE_HDR table and the appropriate sourcing rule can be selected.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="GiftFlag" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="GIFT_FLAG" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This flag indicates whether the order line is a gift item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="GiftWrap" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_GIFT_WRAP" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This field indicates if the order line has to be gift wrapped.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HoldFlag" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="HOLD_FLAG" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This flag determines whether a hold has been placed on the order when certain hold type criteria are met. If order is on hold, default configuration will stop the shceduling and release process for the order. The flag is reset to 'N' when all the hold types have been resolved on the order and all of its order lines. Based on hold type configuration, certain transactions may not be allowed to process. Note that there is no validation made on the hold applied but this flag can be reset by the application based on hold types existed on the order. Default: N Valid Values: Y or N</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HoldReasonCode" type="yfctype:HoldReasonCode" use="optional">
                <xsd:annotation yfc:columnName="HOLD_REASON_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This field is not currently used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ImportLicenseExpDate" type="yfctype:Date" use="optional">
                <xsd:annotation yfc:columnName="IMPORT_LICENSE_EXP_DATE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The expiration date of the import license number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ImportLicenseNo" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="IMPORT_LICENSE_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The import license number to be used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IntentionalBackorder" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="INTENTIONAL_BACKORDER" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
						This flag indicates whether the order line should be backordered upon creation. If passed as 'Y', the line will be backordered even if inventory is available. If a reservation ID is passed on the order line and reservation exists for a current, past or future date, reservation will be consumed to block inventory for the backordered demand. In this scenario, demand created by the reservation ID is removed, and a backordered demand will be generated. No attempt is made to look for inventory if reservation is not available for the passed reservation ID. For draft orders, the reservation is consumed and the order line is created in created status. During confirmDraftOrder, this line goes in backordered status. This flag is ignored if reservation is mandatory. This flag should not be passed for bundle parent lines. If it is passed as 'Y' for bundle parent order line, the line is created in backorder status. It is not propagated to bundle components.
						 </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsFirmPredefinedNode" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_FIRM_PREDEFINED_NODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
                    Indicates if the pre-defined node on the line is firm or not.  If IsFirmPredefinedNode is marked as <b>N</b>, then the node on the line is ignored during scheduling. If not passed, the default value for this Flag is <b>Y</b>. If <code>ProcureFromNode</code> is passed, this flag will always be set to <b>Y</b>. For a <b>DS</b> or a <code>PS</code> line so, it is defaulted to <b>Y</b>. If the ship node is not present on the line but the value passed is <b>Y</b>, then it is treated as <b>N</b> during scheduling.
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsForwardingAllowed" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_FORWARDING_ALLOWED" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This flag indicates whether forwarding is allowed for the line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsProcurementAllowed" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_PROCUREMENT_ALLOWED" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This flag indicates whether procurement is allowed for the line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemGroupCode" type="yfctype:ItemGroupCode" use="optional">
                <xsd:annotation yfc:columnName="ITEM_GROUP_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
						Specifies whether the <code>OrderLine</code> is a
						product,  delivery, or provided service Line.
						<br/>Default: <b>
                                <code>PROD</code>
                            </b> - <i>Product</i>
                            <br/>Valid Values: <b>
                                <code>PROD</code>
                            </b>, <b>
                                <code>DS</code>
                            </b> or <b>
                                <code>PS</code>
                            </b>.
						<br/>If the <code>ItemGroupCode</code> is not passed, the <code>Item</code> is looked up in the catalog or is assigned during the user exit <code>YCMGetItemDetailsUE</code>. If the <code>Item</code> is not found, the <code>ItemGroupCode</code> is defaulted to <b>
                                <code>PROD</code>
                            </b>.
						<Validation>
                                <Condition>If the value is passed, it must be a valid value.
							</Condition>
                                <Error Description="Invalid Item Group Code Passed" ErrorCode="OMP10019"/>
                            </Validation>
                            <Validation>
                                <Condition>When service item which exists in the catalog is passed for bundle parent line.
						</Condition>
                                <Error Description="Bundle parent item cannot be a service item." ErrorCode="OMP80_0005"/>
                            </Validation>
                            <Validation>
                                <Condition>When DS item is passed as component for a bundle.
						</Condition>
                                <Error Description="A bundle cannot have DS component" ErrorCode="OMP80_0015"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="KitCode" type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="KIT_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
                            <br/>Specifies a collection of items that form a unit. For example, a home theater system is a unit, or kit, consisting of components such as a VCR, a TV, a stereo, and other components.
						 <br/>Valid values:  <b>
                                <code>LK</code>
                            </b> - <i>Logical kit</i>, <b>
                                <code>PK</code>
                            </b> - <i>Physical kit</i>, <b>
                                <code>DPK</code>
                            </b> - <i>Dynamic Physical kit</i>, <b>
                                <code>BUNDLE</code>
                            </b> - <i>Bundle</i>
                            <br/> If no KitCode is passed, the <code>createOrder</code> API searches the item in the Catalog. The following are the possible scenarios.  See Validation Conditions for possible error scenarios.
						 <br/>
                            <table border="0" cellpadding="0" cellspacing="0" class="table">
                                <tbody>
                                    <tr>
                                    <td class="tablecolumn" width="20%">
                                    <b>Kit definition passed</b>
                                    </td>
                                    <td class="tablecolumn" width="20%">
                                    <b>Item in catalog</b>
                                    </td>
                                    <td class="tablecolumn" width="20%">
                                    <b>KitItem</b>
                                    </td>
                                    <td class="tablecolumn" width="40%">
                                    <b>Orderline based on</b>
                                    </td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn" width="20%">Y</td>
                                    <td class="tablecolumn" width="20%">Y </td>
                                    <td class="tablecolumn" width="20%">Y</td>
                                    <td class="tablecolumn" width="40%">Passed KitDefinition</td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn" width="20%">N</td>
                                    <td class="tablecolumn" width="20%">Y </td>
                                    <td class="tablecolumn" width="20%">Y</td>
                                    <td class="tablecolumn" width="40%">Catalog definition</td>
                                    </tr>
                                </tbody>
                            </table>
                            <Validation>
                                <Condition>Kit definition not passed, item exists in the catalog, it is a kit item, but there is no kit definition.
							</Condition>
                                <Error Description="YFS: No Kit Lines Found" ErrorCode="YFS10273"/>
                            </Validation>
                            <Validation>
                                <Condition>Kit definition not passed, item does not exist in the catalog.
							</Condition>
                                <Error Description="YFS: No Kit Lines Found" ErrorCode="YFS10273"/>
                            </Validation>
                            <Validation>
                                <Condition>Kit definition passed as the KitLines element, and the Item is not in catalog.
							</Condition>
                                <Error Description="Invalid kit code" ErrorCode="YFS10442"/>
                            </Validation>
                            <Validation>

                                <Condition>Kit definition passed as the KitLines element, and the Item is not a KitItem.
							<Error Description="Invalid kit code" ErrorCode="YFS10442"/>
                                </Condition>
                            </Validation>
                            <Validation>
                                <Condition>Bundle parent line can not have a logical kit component.
							 </Condition>
                                <Error Description="Logical kit component not supported for bundle" ErrorCode="OMP80_0001"/>
                            </Validation>
                            <Validation>
                                <Condition>Bundle parent line can not have a dynamic physical kit component.
							 </Condition>
                                <Error Description="Dynamic physical kit component not supported for bundle" ErrorCode="OMP79_0023"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="KitQty" use="optional">
                <xsd:annotation yfc:columnName="KIT_QTY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The quantity that is used to compute ordered quantity for bundle component line. It can be specified only for bundle components. If specified ordered quantity will be set to KitQty*OrderedQty of the bundle parent.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Quantity"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="LastIterationSeqNo" use="optional">
                <xsd:annotation yfc:columnName="LAST_ITER_SEQ_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The last iteration sequence for which this line is applicable. If Null, it is assumed to go on indefinitely. Default is Null.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:LongSequence"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="LevelOfService" type="yfctype:LevelOfService" use="optional">
                <xsd:annotation yfc:columnName="LEVEL_OF_SERVICE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>Indicates the level of service required for this order line.  Default is Null.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LineType" type="yfctype:LineType" use="optional">
                <xsd:annotation yfc:columnName="LINE_TYPE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The line type. This field can be used in process modeling for pipeline determination, or for doing conditional processing for this line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MergeNode" type="yfctype:ShipNode" use="optional">
                <xsd:annotation yfc:columnName="MERGE_NODE" yfc:isConditional="Y" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
                            <br/>If two or more <code>OrderLines</code> are to be delivered together, this is the node where these lines are merged before final delivery. This field is required when the <code>DependencyShippingRule</code> is set to <b>
                                <code>02</code>
                            </b>.
						<Validation>
                                <Condition>Must exist in table <code>YFS_SHIP_NODE</code>.
							 </Condition>
                                <Error Description="Invalid ship node" ErrorCode="YFS10048"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MinShipByDate" use="optional">
                <xsd:annotation yfc:columnName="MIN_SHIP_BY_DATE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
                        The preferred minimum ship by date of the available supply that order line can allocate against.  Available supply with ship by date earlier than the minimum ship by date will not be considered.  This applies to the order line with a valid time-sensitive item.  The <code>MinShipByDate</code> of the order line will also be used by all kit lines with valid time-sensitive item.
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:TimeStamp"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="OrderedQty" type="yfctype:Quantity" use="optional">
                <xsd:annotation yfc:columnName="ORDERED_QTY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
						The quantity of the line item that is being ordered. This quantity is in Inventory UnitOfMeasure. If Quantity needs to be passed in a different UnitOfMeasure (only allowed for a Product line), then it should be passed in OrderLineTranQuantity node.
						<br/> For a service line item which is used as a standalone service, <code>OrderedQty</code> is treated in the same manner as product lines and must be a non-zero positive number.
						<br/>The <code>OrderedQty</code> value passed on the service line is ignored, if ordered quantity computaion is not based on entered quantity.
						 <Validation>
                                <Condition>Any non-zero positive number for a product Line or standalone service line.
							 </Condition>
                                <Error Description="java.lang.NumberFormatException" ErrorCode=""/>
                            </Validation>
                            <Validation>
                                <Condition>If <code>SerialNo</code> is provided and <code>OrderedQty</code> is not equal to one.
							  </Condition>
                                <Error Description="OrderedQty must be equal to one because SerialNo is provided." ErrorCode="OMP11024"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PackListType" type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="PACKLIST_TYPE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The code indicating the type of packlist to be used for the line. Lines requiring different pack lists are not mixed in a single order release.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PersonalizeCode" type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="PERSONALIZE_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The code for personalization.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PickableFlag" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="PICKABLE_FLAG" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This field is used internally.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PipelineId" type="yfctype:Identifier" use="optional">
                <xsd:annotation yfc:columnName="PIPELINE_ID" yfc:tableName="YFS_PIPELINE">
                    <xsd:documentation>
                        <div>
					Pipeline id on order line. If not passed, system will determine the pipeline based on attributes on the line.
					<Validation>
                                <Condition>
						It should be a valid pipeline for the process type order line belongs.
						</Condition>
                                <Error Description="Invalid pipeline id" ErrorCode="OMP11071"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PrimeLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="PRIME_LINE_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
						The order line number. Part of an <code>OrderLine</code>'s unique key.
						If <code>PrimeLineNo</code> is not passed, it is generated. A starting number can be specified  in the configurator at each document type and enterprise level. If a starting number is specified the <code>PrimeLineNo</code> will be generated starting from that number.
						</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ProcureFromNode" type="yfctype:ShipNode" use="optional">
                <xsd:annotation yfc:columnName="PROCURE_FROM_NODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
                    The node from which the order line should be procured if cannot source from the shipnode on the order line.
                    <Validation>
                                <Condition>
                        ProcureFromNode can be specified only if shipnode is passed.
                        </Condition>
                                <Error Description="Cannot pass ProcureFromNode without Shipnode" ErrorCode="OMP10086"/>
                            </Validation>
                            <Validation>
                                <Condition>
                        If Reservation is attempted from ProcureFromNode, then shipnode must have <code>Procure To Ship Allowed</code> Flag set to <b>Y</b>
                                </Condition>
                                <Error Description="Cannot pass ProcureFromNode when ShipNode cannot procure." ErrorCode="OMP11042_55SP1"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PromisedApptEndDate" use="optional">
                <xsd:annotation yfc:columnName="PROMISED_APPT_END_DATE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
						The date and time when the appointment ends for an <code>OrderLine</code>.This date only applies to Provided (ItemGroupCode=PS) and Delivered (ItemGroupCode=DS) lines.
						<Validation>
                                <Condition>
                                    <code>PromisedApptEndDate</code> must be greater than or equal to <code>PromisedApptStartDate</code>.
							</Condition>
                                <Error Description="PromisedApptStartDate cannot be greater than PromisedApptEndDate" ErrorCode="OMP10056"/>
                            </Validation>
                            <Validation>
                                <Condition>If <code>PromisedApptEndDate</code> is passed, but  <code>PromisedApptStartDate</code> is not passed.
							</Condition>
                                <Error Description="Both PromisedApptStartDate and PromisedApptEndDate must be passed, if either one is passed." ErrorCode="OMP11047_55SP2"/>
                            </Validation>
                            <Validation>
                                <Condition>If the date corresponds to a slot which cannot be used for appointment planning.
							</Condition>
                                <Error Description="Slot based Capacity Consumption cannot be done on slots that cannot be used for taking appointments." ErrorCode="INV80_002"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:TimeStamp"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="PromisedApptStartDate" use="optional">
                <xsd:annotation yfc:columnName="PROMISED_APPT_START_DATE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>	The date and time when the appointment starts for an <code>OrderLine</code>. This date only applies to Provided (ItemGroupCode=PS) and Delivered (ItemGroupCode=DS) lines.
						<Validation>
                                <Condition>
                                    <code>PromisedApptStartDate</code> cannot be after <code>PromisedApptEndDate</code>.
							</Condition>
                                <Error Description="PromisedApptStartDate cannot be greater than PromisedApptEndDate." ErrorCode="OMP10056"/>
                            </Validation>
                            <Validation>
                                <Condition>If <code>PromisedApptStartDate</code> is passed, but  <code>PromisedApptEndDate</code> is not passed.
                            </Condition>
                                <Error Description="Both PromisedApptStartDate and PromisedApptEndDate must be passed, if either one is passed." ErrorCode="OMP11047_55SP2"/>
                            </Validation>
                            <Validation>
                                <Condition>If the date corresponds to a slot which cannot be used for appointment planning.
							</Condition>
                                <Error Description="Slot based Capacity Consumption cannot be done on slots that cannot be used for taking appointments." ErrorCode="INV80_002"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:TimeStamp"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="Purpose" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="PURPOSE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This field is passed out in the Tax calculation user exits.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReceivingNode" type="yfctype:ShipNode" use="optional">
                <xsd:annotation yfc:columnName="RECEIVING_NODE" yfc:isConditional="Y" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
						The node to which an <code>OrderLine</code> is to be shipped.<br>
						Note: When calling this API for a return order, this ReceivingNode attribute should contain the receiving_node ID associated with the "Ship From" node (the node from which the returned item is shipped).</br>
                            <Validation>
                                <Condition>Must exist in table <code>YFS_SHIP_NODE</code>.
							</Condition>
                                <Error Description="Invalid Ship node" ErrorCode="YFS10048"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReqCancelDate" use="optional">
                <xsd:annotation yfc:columnName="REQ_CANCEL_DATE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The requested cancel date. If not provided, this field is defaulted from the order header during order creation. Refer to the YFS_ORDER_HEADER table for more details.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:TimeStamp"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="ReqDeliveryDate" use="optional">
                <xsd:annotation yfc:columnName="REQ_DELIVERY_DATE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The requested delivery date. If not provided, this field is defaulted from the order header during order creation. Refer to the YFS_ORDER_HEADER table for more details.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:TimeStamp"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="ReqShipDate" use="optional">
                <xsd:annotation yfc:columnName="REQ_SHIP_DATE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The requested ship date. If not provided, this field is defaulted from the order header during order creation. Refer to the YFS_ORDER_HEADER table for more details.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:TimeStamp"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="ReservationID" type="yfctype:ID-40" use="optional">
                <xsd:annotation yfc:columnName="RESERVATION_ID" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The reservation ID. If this item was reserved before order creation, pass an ID to associate the line with the reservation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReservationMandatory" use="optional">
                <xsd:annotation yfc:columnName="RESERVATION_MANDATORY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
						This flag indicates whether this line can be created without a reservation. If set to Y, creation of this line or any quantity addition to this line will be allowed only if the quantity can be reserved. If a ProcureFromNode is also present on the line, then the reservation will be carried out on the procure from node. This flag is not supported by bundle parent. It is ignored by bundle parent order lines. It is not propagated to bundle components.
						 </div>
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="yfctype:Flag">
                        <xsd:enumeration value="Y"/>
                        <xsd:enumeration value="N"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="ReturnReason" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="RETURN_REASON" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The reason for returning the order line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SCAC" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="SCAC" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The carrier to be used for this line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ScacAndService" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="SCAC_AND_SERVICE" yfc:tableName="YFS_SCAC_AND_SERVICE">
                    <xsd:documentation>
                        <div>Value of SCAC and Service code.
							<br/>If <code>ScacAndService</code> or <code>ScacAndServiceKey</code> is passed, the values of <code>SCAC</code> and <code>CarrierServiceCode</code> are overriden based on the corresponding values in the <code>YFS_SCAC_AND_SERVICE</code> table.
							<Validation>
                                <Condition>
									Must be an existing ScacAndService code in the table <code>YFS_SCAC_AND_SERVICE</code>.
								</Condition>
                                <Error Description="YFS:Invalid SCAC/Service code combination" ErrorCode="YFS10240"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ScacAndServiceKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="SCAC_AND_SERVICE_KEY" yfc:tableName="YFS_SCAC_AND_SERVICE">
                    <xsd:documentation>Primary key attribute of YFS_SCAC_AND_SERVICE.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Segment" type="yfctype:CommonCode" use="optional">
                <xsd:annotation yfc:columnName="SEGMENT" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
						Indicates Segment for the order line. For a Made To Customer order, the value is defaulted to <code>BuyerOrganizationCode</code>. For a Made To Order Order, the value is defaulted to <code>OrderLineKey</code>.
						</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SegmentType" type="yfctype:ID-10" use="optional">
                <xsd:annotation yfc:columnName="SEGMENT_TYPE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
						Indicates SegmentType for the order line. For a Made to Customer Order, the value is <b>MTC</b>. For a Dynamic Physical Kit item which is Made To Order, SegmentType is <b>MTO</b>. For an MTC order, Inventory Tag passed at line level is used. Inventory Tags passed at schedule level are not used for creation of work orders.
						<br/>If passed as MTC, the following validations are done:
						<br/>a) Buyer Organization must have <code>Requires_Vas_Compliance</code> flag set to <b>Y</b>
                            <br/>b) Buyer Organization must have have Inbound Vas Compliance setup for the item passed in Order Line.
						<br/>c) The Service item selected must have Involves_Segment_Change flag set to <b>Y</b>.
						<br/>
                            <Validation>
                                <Condition>If <code>SegmentType</code> is <b>MTC</b>, Buyer Organization must have VAS Compliance setup as specified above.
							</Condition>
                                <Error Description="Cannot Pass SegmentType as MTC for a Buyer that does not require VAS Compliance" ErrorCode="OMP11031"/>
                            </Validation>
                            <br/>If passed as MTO, the following validations are done:
						<br/>a) The KitCode passed must be <b>DPK</b>.
						<br/>b) The kit components must be passed.
						<Validation>
                                <Condition>If <code>SegmentType</code> is <b>MTO</b>, the KitCode passed must be <b>DPK</b>
                                </Condition>
                                <Error Description="Invalid Kit Item" ErrorCode="YFS10399"/>
                            </Validation>
                            <Validation>
                                <Condition>If <code>SegmentType</code> is <b>MTO</b>, the kit components must be passed.
							</Condition>
                                <Error Description="YFS: No Kit Lines Found." ErrorCode="YFS10273"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SerialNo" type="yfctype:SerialNo" use="optional">
                <xsd:annotation yfc:columnName="SERIAL_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
                        Requested serial number of the item.
                        <Validation>
                                <Condition>If <code>SerialNo</code> is provided and <code>OrderedQty</code> is not equal to one.
                              </Condition>
                                <Error Description="OrderedQty must be equal to one because SerialNo is provided." ErrorCode="OMP11024"/>
                            </Validation>
                            <Validation>
                                <Condition>If <code>SerialNo</code> is provided but <code>ShipNode</code> is not.
                            </Condition>
                                <Error Description="Ship node is mandatory because SerialNo is provided." ErrorCode="OMP11025"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipNode" type="yfctype:ShipNode" use="optional">
                <xsd:annotation yfc:columnName="SHIPNODE_KEY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
						Ship node from which the order line will be fulfilled.<br>
						Note: When calling this API for a return order, this ShipNode attribute should contain the shipnode_key associated with the "Return To" node (the node to which the returned item is being sent).</br>
                            <Validation>
                                <Condition>Must exist in table <code>YFS_SHIP_NODE</code>
                                </Condition>
                                <Error Description="Invalid ship node" ErrorCode="YFS10048"/>
                            </Validation>
                            <Validation>
                                <Condition>If <code>SerialNo</code> is provided, <code>ShipNode is mandatory</code>
                                </Condition>
                                <Error Description="Ship node is mandatory because SerialNo is provided." ErrorCode="OMP11025"/>
                            </Validation>
                            <Validation>
                                <Condition>If <code>ShipNode</code> is provided and <code>ReservationID</code> is provided to represent a capacity reservation for the service line and <code>ShipNode</code> is not the same as the node of the resource pool on the reservation.
							</Condition>
                                <Error Description="Node does not match with the node of the resource pool of the reservation." ErrorCode="OMP73_10001"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipToID" type="yfctype:ID-40" use="optional">
                <xsd:annotation yfc:columnName="SHIP_TO_ID" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The ship to identifier.  If a customer definition representing the buyer organization exists within the system, the ShipToID can represent the CustomerID of the customer.  Otherwise, the ShipToID can represent the PersonID of the ship to address or the receiving node of the order.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipToKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="SHIP_TO_KEY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The ship to address that is associated with this order line. If not provided, it is defaulted from order header.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipTogetherNo" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="SHIP_TOGETHER_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This field is not currently used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SubLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="SUB_LINE_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The sub line number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Timezone" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="TIMEZONE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
                            <br/>Times from the database are converted into the timezone specified by this field by the UI. Used with order lines with an <code>ItemGroupCode</code> of <b>
                                <code>PS</code>
                            </b> - <i>Provided Service</i> or <b>
                                <code>DS</code>
                            </b> - <i>Delivery Service</i>.
						</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TransactionalLineId" type="yfctype:Text-40" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
                            <code>TransactionalLineId</code> used to identify a line when creating associations of this line with other lines. This <code>TransactionalLineId</code> is not stored in the database and cannot be used after an order has been created. See <code>ProductServiceAssoc</code> entity for more information.
						<Validation>
                                <Condition>Must be unique for the <code>createOrder</code> transaction.
							</Condition>
                                <Error Description="Duplicate TransactionalLineId passed" ErrorCode="OMP10077"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ValidateItem" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
						  Indicates if item needs to be validated against catalog. It overrides configuration rule for validating the item if passed non-blank. When set to "N", this order line will not be considered for min/max ordered quantity validation. Must be set to <code>Y</code> to apply entitlement logic.
						  <br/>Default: <b>
                                <code>Null</code>
                            </b>
                            <br/>Valid Values: <b>
                                <code>Y</code>
                            </b> or <b>
                                <code>N</code>
                            </b>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderLineTranQuantity">
        <xsd:annotation>
            <xsd:documentation>
                <div>
		Describes the OrderLine Transaction Quantity in the TransactionalUOM. Should be passed only if OrderedQty has to be passed in a different UnitOfMeasure than Inventory UnitOfMeasure and hence is not passed as part of <code>OrderLine</code> element.
		</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="FillQuantity" type="yfctype:Quantity" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
			  The minimum order line quantity that must be fulfilled in an order release before it is shipped.
			  </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="KitQty" type="yfctype:LineNo" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
                            <Validation>
                                <Condition>Must be greater than zero if passed.
								</Condition>
                                <Error Description="KitQty for bundle component should be greater than zero if passed." ErrorCode="OMP80_0017"/>
                                <Condition>Number passed has a fraction.
								</Condition>
                                <Error Description="Fractional quantity can not be passed to this attribute." ErrorCode="YFS10447"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrderedQty" type="yfctype:Quantity" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
			  The quantity of the line item that has been ordered. If part of line is cancelled, this number is reduced appropriately. If an order line is returned, this number is not reduced.
			  </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TransactionalUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
			  UOM on which all the transactional quantities are specified.
			  </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="DerivedFrom">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE">
            <xsd:documentation>
                <div>
					Use this element to derive a new <code>OrderLine</code> from an existing <code>OrderLine</code>. If the passed order line is a bundle parent then all the PROD components that have specified <code>KitQty</code> are also pulled in the derived order. If components do not meet status criteria then error is thrown. Bundle-child relationship is maintained in the derived order if parent and components exist in the same derived order.
					<br/>The existing <code>OrderLine</code> can be identified by using one of the following combinations:
					<br/> -<code>OrderLineKey</code>
                    <br/> -<code>OrderHeaderKey</code>, <code>PrimeLineNo</code>, and  <code>SubLineNo</code>
                    <br/> -<code>OrderNo</code>, <code>DocumentType</code> and <code>EnterpriseCode</code>, <code>PrimeLineNo</code>,  and <code>SubLineNo</code>
                    <br/>
                    <br/>If the parent line cannot be identified, an error is thrown.
					<br/>
                    <br/>To select only the part of the existing <code>OrderLine</code> associated with a particular release, to
					derive a new  <code>OrderLine</code> from, additional data is needed. In addition to the information to identify the existing  <code>OrderLine</code>, supply either the <code>OrderReleaseKey</code> or the <code>ReleaseNo</code> that  identifies the release.
					<br/>
                    <br/>Information is taken from the parent line, based on the DerivedOrderDefaultTemplate, stored in the <code>YFS_DOCUMENT_TEMPLATE</code> table. The DerivedOrderDefaultTemplate is chosen on the basis of the DocumentType of the child order.
					<br/>
                    <br/>If there are attributes passed in the  <code>createOrder</code> input xml,  the values in the passed attributes are used creating the new <code>OrderLine</code>. If passed line is a bundle parent line, derived lines are created for all the eligible components existing in the parent order. All the attributes which are passed to <code>OrderLine</code> and its children elements are applied to all the components that are created as part of creating derived line from a bundle parent. If <code>KitCode</code> and <code>Item</code> attributes are changed while creating the derived line from bundle parent, result may be unpredictable.
					<br/>Schedule information is never propagated from the parent line to the child line.
					<br/>
                    <br/>The <code>DerivedFromOrderLineKey</code>, <code>DerivedFromOrderHeaderKey</code> and, if applicable, <code>DerivedFromOrderReleaseKey</code> are set on the new <code>OrderLine</code>. The new <code>OrderLine</code>is validated, like all the other lines of the new order.
					<br/> A transaction is triggered. The transaction is based on the existing order's ProcessType, and the document type of the new order. This IncludeInDerivedOrder transaction changes the status of that quantity of the existing line which is equal to the OrderedQty of the child line. If the parent line does not have sufficient quantity to derive from, an error is thrown.
					</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="DocumentType" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="DOCUMENT_TYPE" yfc:isConditional="Y" yfc:tableName="YFS_ORDER_HEADER">
                    <xsd:documentation>The document type that is associated with this order. For example, Sales Order document (0001), Planned Order document (0002), Return Order document (0003) , and Purchase Order document (0005). This field determines the order's fulfillment cycle and controls the inventory and payment updates performed on the order.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EnterpriseCode" type="yfctype:OrgCode" use="optional">
                <xsd:annotation yfc:isConditional="Y"/>
            </xsd:attribute>
            <xsd:attribute name="OrderHeaderKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORDER_HEADER_KEY" yfc:isConditional="Y" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The order header that this order line is associated with.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrderLineKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORDER_LINE_KEY" yfc:isConditional="Y" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The primary key for the YFS_ORDER_LINE table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrderNo" type="yfctype:DocumentNo" use="optional">
                <xsd:annotation yfc:isConditional="Y"/>
            </xsd:attribute>
            <xsd:attribute name="OrderReleaseKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:isConditional="Y"/>
            </xsd:attribute>
            <xsd:attribute name="PrimeLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="PRIME_LINE_NO" yfc:isConditional="Y" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The order line number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReleaseNo" type="yfctype:Count" use="optional">
                <xsd:annotation yfc:isConditional="Y"/>
            </xsd:attribute>
            <xsd:attribute name="SubLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="SUB_LINE_NO" yfc:isConditional="Y" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The sub line number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ChainedFrom">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE">
            <xsd:documentation>
                <div>
				Optional. Use this element only to chain a new <code>OrderLine</code> with an existing <code>OrderLine</code>.
				<br/>The existing <code>OrderLine</code> can be identified by using one of the following:
				<br/>- <code>OrderLineKey</code>
                    <br/>- <code>OrderHeaderKey</code>, <code>PrimeLineNo</code>, and  <code>SubLineNo</code>
                    <br/>- <code>OrderNo</code>, <code>DocumentType</code>, <code>EnterpriseCode</code>, <code>PrimeLineNo</code>,  and <code>SubLineNo</code>
                    <br/>
                    <br/>If the parent line cannot be identified, an error is thrown.
				<br/>
                    <br/>Information is taken from the parent line, based on the ChainedOrderOtherTemplate, stored in the YFS_DOCUMENT_TEMPLATE table. The ChainedOrderOtherTemplate is chosen on the basis of the DocumentType of the parent order.
				<br/>
                    <br/>If there are attributes passed in the  <code>createOrder</code> input xml,  the values in the passed attributes are used for creating the new <code>OrderLine</code>.
				<br/>Schedule information is never propagated from the parent line to the child line.
				<br/>
                    <br/>The <code>ChainedFromOrderLineKey</code> and <code>ChainedFromOrderHeaderKey</code> are set on the new <code>OrderLine</code>. The new <code>OrderLine</code> is validated, like all the other lines of the new order.
				<br/> A transaction is triggered. The transaction is based on the existing order's ProcessType, and the document type of the new order. This <b>IncludeInChainedOrder</b> transaction changes the status of that quantity of the existing line which is equal to the OrderedQty of the child line. If the parent line does not have sufficient quantity to chain from, error is thrown.
				</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="DocumentType" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="DOCUMENT_TYPE" yfc:tableName="YFS_ORDER_HEADER">
                    <xsd:documentation>The document type that is associated with this order. For example, Sales Order document (0001), Planned Order document (0002), Return Order document (0003) , and Purchase Order document (0005). This field determines the order's fulfillment cycle and controls the inventory and payment updates performed on the order.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EnterpriseCode" type="yfctype:OrgCode" use="optional">
                <xsd:annotation>
                    <xsd:documentation>The enterprise that is associated with this order.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrderHeaderKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORDER_HEADER_KEY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The order header that this order line is associated with.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrderLineKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORDER_LINE_KEY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The primary key for the YFS_ORDER_LINE table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrderNo" type="yfctype:DocumentNo" use="optional">
                <xsd:annotation>
                    <xsd:documentation>The order number that this order line is chained from.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PrimeLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="PRIME_LINE_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The order line number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SubLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="SUB_LINE_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The sub line number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderLineOptions">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrderLineOption"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderLineOption">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE_OPTION">
            <xsd:documentation>
                <div>
				Describes the Options attached to a <code>OrderLine</code>.
				Options can only be specified for a Delivery or a Service <code>OrderLine</code>.
				<Validation>
                        <Condition>An <code>OrderLineOption</code> is passed for a Product <code>OrderLine</code>.
					</Condition>
                        <Error Description="An Order Line with ItemGroupCode equalTo PROD cannot have Options" ErrorCode="OMP10029"/>
                    </Validation>
                </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="OptionDescription" type="yfctype:FreeText-200" use="optional">
                <xsd:annotation yfc:columnName="OPTION_DESCRIPTION" yfc:tableName="YFS_ORDER_LINE_OPTION">
                    <xsd:documentation>Short Description for the Option added to the Item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OptionItemID" type="yfctype:ItemID" use="required">
                <xsd:annotation yfc:columnName="OPTION_ITEM_ID" yfc:tableName="YFS_ORDER_LINE_OPTION">
                    <xsd:documentation>
                        <div>
					The <code>ItemID</code> of the <code>Item</code> with an Option.
					<Validation>
                                <Condition>Must be a valid <code>OptionItemID</code>.
						</Condition>
                                <Error Description="Invalid OptionItemId passed for the Line Item." ErrorCode="OMP10201"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OptionUOM" type="yfctype:UOM" use="required">
                <xsd:annotation yfc:columnName="OPTION_UOM" yfc:tableName="YFS_ORDER_LINE_OPTION">
                    <xsd:documentation>Unit Of Measure of the Option Item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PricingUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="PRICING_UOM" yfc:tableName="YFS_ORDER_LINE_OPTION">
                    <xsd:documentation>For future use.  Option is currently priced based on ordering unit of measure.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitPrice" type="yfctype:UnitPrice" use="optional">
                <xsd:annotation yfc:columnName="UNIT_PRICE" yfc:tableName="YFS_ORDER_LINE_OPTION">
                    <xsd:documentation>Unit price for the Option.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderLineSourcingControls">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:OrderLineSourcingCntrl"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderLineSourcingCntrl">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE_SRC_CNTRL">
            <xsd:documentation>
                <div>
            Describes the Sourcing Controls attached to a <code>OrderLine</code>.
					 <Validation>
                        <Condition>If specified for bundle parent line - for line having KitCode as BUNDLE
						</Condition>
                        <Error Description="Bundle parent line can not have OrderLineSourcingCntrl element." ErrorCode="OMP80_0004"/>
                    </Validation>
                </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="InventoryCheckCode" type="yfctype:EnumInvCheckCode" use="optional">
                <xsd:annotation yfc:columnName="INV_CHECK_CODE" yfc:tableName="YFS_ORDER_LINE_SRC_CNTRL">
                    <xsd:documentation>If this attribute is populated, inventory availability for that node is overridden based on the value.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Node" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="NODE_KEY" yfc:tableName="YFS_ORDER_LINE_SRC_CNTRL">
                    <xsd:documentation>The node for which either sourcing or procurement has been suppressed when either Suppress Sourcing or Suppress Procurement flags are set to Yes. NOTE: If Node is blank and SuppressNodeCapacity is Yes, then inventory node capacity will be ignored for all nodes of the OrderLine.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReasonText" type="yfctype:ReasonText" use="optional">
                <xsd:annotation yfc:columnName="REASON_TEXT" yfc:tableName="YFS_ORDER_LINE_SRC_CNTRL">
                    <xsd:documentation>Free-form reason text indicating any additional information.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SuppressNodeCapacity" use="optional">
                <xsd:annotation yfc:columnName="SUPPRESS_NODE_CAPACITY" yfc:tableName="YFS_ORDER_LINE_SRC_CNTRL">
                    <xsd:documentation>Flag determines whether to ignore inventory node capacity. Default: N. NOTE: If Node is blank and SuppressNodeCapacity is Yes, then inventory node capacity will be ignored for all nodes of the OrderLine.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="yfctype:Flag">
                        <xsd:enumeration value="N"/>
                        <xsd:enumeration value="Y"/>
                        <xsd:enumeration value=""/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="SuppressProcurement" use="optional">
                <xsd:annotation yfc:columnName="SUPPRESS_PROCUREMENT" yfc:tableName="YFS_ORDER_LINE_SRC_CNTRL">
                    <xsd:documentation>Flag determines whether to ignore this Node for procurement consideration.  Node will not be used by scheduling as a procure from node.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="yfctype:Flag">
                        <xsd:enumeration value="Y"/>
                        <xsd:enumeration value="N"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="SuppressSourcing" use="optional">
                <xsd:annotation yfc:columnName="SUPPRESS_SOURCING" yfc:tableName="YFS_ORDER_LINE_SRC_CNTRL">
                    <xsd:documentation>Flag determines whether to ignore this Node for sourcing consideration.  Node will not be used by scheduling as a ship node.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="yfctype:Flag">
                        <xsd:enumeration value="Y"/>
                        <xsd:enumeration value="N"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Item">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE">
            <xsd:documentation>
                <div>
			 Contains information regarding the item for which order is created. Service items should be defined in the system. If ItemID is not passed, the catalog will be searched for the item which has the same alias as the AlternateItemID. The passed UOM will also be used to search for the item in the catalog. If an item is not found, the AlternateItemID will be used as the ItemID.
				 <Validation>
                        <Condition>If item needs to be validated (based on process type attribute or if ItemGroupCode is PS or DS) then it need to be defined in YFS_ITEM, or be accessible through the getItemDetails user exit.
				</Condition>
                        <Error Description="Item (ItemId and UOM) does not exist" ErrorCode="YFS10437"/>
                    </Validation>
                </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="AlternateItemID" type="yfctype:ItemID" use="optional">
                <xsd:annotation yfc:columnName="ALTERNATE_ITEM_ID" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This field indicates the alternate ItemID, which can be passed instead of the actual ItemID. This is an alias for the ItemID.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="BundleFulfillmentMode" use="optional">
                <xsd:annotation yfc:columnName="BUNDLE_FULFILLMENT_MODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The way in which the bundle will be fulfilled. It can be Ship Independent (00), Ship Together (01), or Deliver Together (02) for Bundle Item. Default value is based on the catalog. If the item is not in the catalog, then Ship Together(01) is defaulted for Bundle Item.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="yfctype:Text-2">
                        <xsd:enumeration value=""/>
                        <xsd:enumeration value="00"/>
                        <xsd:enumeration value="01"/>
                        <xsd:enumeration value="02"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="CostCurrency" type="yfctype:Currency" use="optional">
                <xsd:annotation yfc:columnName="COST_CURRENCY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The currency in which the unit cost is specified.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CountryOfOrigin" type="yfctype:Country" use="optional">
                <xsd:annotation yfc:columnName="COUNTRY_OF_ORIGIN" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>
                        <div>
					The <code>Item</code>'s  country of origin/manufacture. We recommend usage of 2 byte ISO Country Codes.
					</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CustomerItem" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="CUSTOMER_ITEM" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The item ID for the buyer.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CustomerItemDesc" type="yfctype:Text-500" use="optional">
                <xsd:annotation yfc:columnName="CUSTOMER_ITEM_DESCRIPTION" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The item description of the buyer organization.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ECCNNo" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="ECCN_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The Export Commodity Control Number for a commodity contained in the shipment, as indicated on the Waybill.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HarmonizedCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="HARMONIZED_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The harmonized code, if applicable.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ISBN" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="ISBN" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The ISBN number, if applicable.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemDesc" type="yfctype:Text-500" use="optional">
                <xsd:annotation yfc:columnName="ITEM_DESCRIPTION" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The description of the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemID" type="yfctype:ItemID" use="optional">
                <xsd:annotation yfc:columnName="ITEM_ID" yfc:isConditional="Y" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The identifier for the item. The ITEM_ID, UOM, and PRODUCT_CLASS form the unique inventory identifier for an order line. The ITEM_ID, UOM and ORGANIZATION_CODE form the unique item identifier for the Product Management module.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemShortDesc" type="yfctype:Text-200" use="optional">
                <xsd:annotation yfc:columnName="ITEM_SHORT_DESCRIPTION" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The short description of the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemWeight" type="yfctype:Dimension" use="optional">
                <xsd:annotation yfc:columnName="ITEM_WEIGHT" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The weight of the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemWeightUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="ITEM_WEIGHT_UOM" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The unit of measure used for weight.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ManufacturerItem" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="MANUFACTURER_ITEM" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The item identifier for the manufacturer.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ManufacturerItemDesc" type="yfctype:Text-254" use="optional">
                <xsd:annotation yfc:columnName="MANUFACTURER_ITEM_DESC" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The item description for the manufacturer.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ManufacturerName" type="yfctype:Text-50" use="optional">
                <xsd:annotation yfc:columnName="MANUFACTURER_NAME" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The name of the manufacturer.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NMFCClass" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="NMFC_CLASS" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The National Motor Freight Classification - A way of classifying goods for the purposes of transportation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NMFCCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="NMFC_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The National Motor Freight Classification - A way of classifying goods for the purposes of transportation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NMFCDescription" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="NMFC_DESCRIPTION" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The description of the NMFC. National Motor Freight Classification is a way of classifying goods for the purposes of transportation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ProductClass" type="yfctype:ProductClass" use="optional">
                <xsd:annotation yfc:columnName="PRODUCT_CLASS" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The product class. This field is used as an inventory identifier.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ProductLine" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="PRODUCT_LINE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The product line for the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ScheduleBCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="SCHEDULE_B_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The Schedule B commodity codes - A 10-digit numeric codes used to identify products that are exported to other countries.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SupplierItem" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="SUPPLIER_ITEM" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The item ID of the supplier (if this is a PO or the line is being drop-shipped and the supplier is known upfront).</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SupplierItemDesc" type="yfctype:ItemDesc" use="optional">
                <xsd:annotation yfc:columnName="SUPPLIER_ITEM_DESCRIPTION" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The item description of the supplier (if this is a PO or line is being drop-shipped and the supplier is known upfront).</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxProductCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="TAX_PRODUCT_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The product code classification for the purposes of taxation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UPCCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="UPC_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The UPC Code of the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitCost" type="yfctype:UnitPrice" use="optional">
                <xsd:annotation yfc:columnName="UNIT_COST" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The unit cost of the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitOfMeasure" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="UOM" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The unit of measure.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="LinePriceInfo">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE">
            <xsd:documentation>This table stores order line information, including item details and shipping information. Multiple lines can be created for a single order defined in the YFS_ORDER_HEADER table.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="DiscountPercentage" type="yfctype:Percentage" use="optional">
                <xsd:annotation yfc:columnName="DISCOUNT_PERCENTAGE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The discount percentage is the percentage decrease in price between the list and unit price for a given buyer. This value is calculated by subtracting the difference between the list price and unit price and divided by the list price.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsLinePriceForInformationOnly" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_PRICE_INFO_ONLY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
								This flag indicates whether price information passed on the line is for information only. If it is 'Y' then line total will not be considered in order total. If it is not passed or passed as 'N' then line total is considered in order total.
								If this flag is not passed for a static bundle (bundle defined in the catalog),
								the the flag will be set on the parent and its components based on the
								<code>BundlePricingStrategy</code> defined on the item.
								<br/>
                            <table border="0" cellpadding="0" cellspacing="0" class="table">
                                <tr>
                                    <th class="tablecolumn">BundlePricingStrategy</th>
                                    <th class="tablecolumn">Bundle Parent Value</th>
                                    <th class="tablecolumn">Components Value</th>
                                </tr>
                                <tr>
                                    <td class="tablecolumn">COMP</td>
                                    <td class="tablecolumn">Y</td>
                                    <td class="tablecolumn">Null (or N)</td>
                                </tr>
                                <tr>
                                    <td class="tablecolumn">PARENT</td>
                                    <td class="tablecolumn">Null (or N)</td>
                                    <td class="tablecolumn">Y</td>
                                </tr>
                                <tr>
                                    <td class="tablecolumn">ALL</td>
                                    <td class="tablecolumn">Null (or N)</td>
                                    <td class="tablecolumn">Null (or N)</td>
                                </tr>
                            </table>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsPriceLocked" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_PRICE_LOCKED" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This flag indicates whether UnitPrice passed at LinePriceInfo element must be used and price determination ignored, if price determination is configured and enabled. Valid values are 'Y' or 'N'. Default value is 'N'.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ListPrice" type="yfctype:UnitPrice" use="optional">
                <xsd:annotation yfc:columnName="LIST_PRICE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>the list price of the item. This is an informational field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PricingQtyConversionFactor" type="yfctype:Quantity" use="optional">
                <xsd:annotation yfc:columnName="PRICING_QUANTITY_CONV_FACTOR" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The pricing quantity conversion factor. This field is used for finding Pricing_Quantity, using Ordered_Quantity.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PricingQuantityStrategy" type="yfctype:EnumPricingQuantityStrategy" use="optional">
                <xsd:annotation yfc:columnName="PRICING_QUANTITY_STRATEGY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
				Pricing Quantity Strategy. Can be Fixed <code>
                                <b>FIX </b>
                            </code>or Incremental <code>
                                <b>IQTY </b>
                            </code>
                            <Validation>
                                <Condition>If incorrect value is passed through the input xml.
					</Condition>
                                <Error Description="Invalid Pricing Quantity Strategy for Product Line Item" ErrorCode="OMP10034"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PricingUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="PRICING_UOM" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The pricing UOM for the order line. If "PricingUOM" is not passed it will be defaulted from Catalog. If "PricingUOM" is passed and different from inventory uom, "PricingQtyConversionFactor" attribute will be used for pricing quantity conversion. If "PricingQtyConversionFactor" is not passed it will be considered as zero.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RetailPrice" type="yfctype:UnitPrice" use="optional">
                <xsd:annotation yfc:columnName="RETAIL_PRICE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The retail price of the item. this is an informational field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxExemptionCertificate" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="TAX_EXEMPTION_CERTIFICATE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The Tax Exemption Certificate applied to this order line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxableFlag" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="TAXABLE_FLAG" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This flag indicates whether this order line is taxable or not. This field is for informational purposes only. Currently, it is not used in any tax calculations. If not passed, this flag is defaulted to that of item in the catalog, if exists, else it is defaulted to "N".</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitPrice" type="yfctype:UnitPrice" use="optional">
                <xsd:annotation yfc:columnName="UNIT_PRICE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The unit price for the order line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="LineCharges">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:LineCharge"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="LineCharge">
        <xsd:annotation yfc:tableName="YFS_LINE_CHARGES">
            <xsd:documentation>This table stores any additional charges or discounts that can be added to an  order line.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ChargeCategory" type="yfctype:ChargeCategory" use="required">
                <xsd:annotation yfc:columnName="CHARGE_CATEGORY" yfc:tableName="YFS_LINE_CHARGES">
                    <xsd:documentation>Link to YFS_CHARGE_CATEGORY.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ChargeName" type="yfctype:ChargeName" use="optional">
                <xsd:annotation yfc:columnName="CHARGE_NAME" yfc:tableName="YFS_LINE_CHARGES">
                    <xsd:documentation>Link to YFS_CHARGE_NAME.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ChargePerLine" type="yfctype:PlusMinusMoney" use="optional">
                <xsd:annotation yfc:columnName="CHARGEPERLINE" yfc:tableName="YFS_LINE_CHARGES">
                    <xsd:documentation>Fixed charge amount for this order/invoice line. When the line charge is associated with an order line, this amount will be invoiced on the first invoice for this order line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ChargePerUnit" type="yfctype:UnitPrice" use="optional">
                <xsd:annotation yfc:columnName="CHARGEPERUNIT" yfc:tableName="YFS_LINE_CHARGES">
                    <xsd:documentation>Charge per unit for the item on this order/invoice line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsManual" type="yfctype:EnumIsManualCharge" use="optional">
                <xsd:annotation yfc:columnName="IS_MANUAL" yfc:tableName="YFS_LINE_CHARGES">
                    <xsd:documentation>Determines whether the charge was manually created. If null, then assumed to be Y.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Reference" type="yfctype:Text-50" use="optional">
                <xsd:annotation yfc:columnName="REFERENCE" yfc:tableName="YFS_LINE_CHARGES">
                    <xsd:documentation>Any additional reference related to this line charge.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="LineTaxes">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:LineTax"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="LineTax">
        <xsd:annotation yfc:tableName="YFS_TAX_BREAKUP">
            <xsd:documentation>
                <div>
						Contains tax related information for various entities in the system.  If <code>ChargeCategory</code> and <code>ChargeName</code> are not provided, tax will be considered as a billable and positive charge.  If not implementing <code>YFSRecalculateLineTaxUE</code>, <code>ChargeCategory</code>, <code>Tax</code>, and <code>TaxPercentage</code> are required for the default repricing logic.
						</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ChargeCategory" type="yfctype:ChargeCategory" use="optional">
                <xsd:annotation yfc:columnName="CHARGE_CATEGORY" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>
							The charge category for which tax is being charged. For example, Labeling or Handling.
							To specify the tax for the line price, use the charge category  <b>
                            <code>Price</code>
                        </b>.
							</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ChargeName" type="yfctype:ChargeName" use="optional">
                <xsd:annotation yfc:columnName="CHARGE_NAME" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Charge name for which tax is being charged. For example, Price Ticket.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Reference1" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="REFERENCE1" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Additional reference information. This field can be customized by the Enterprise.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Reference2" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="REFERENCE2" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Additional reference information. This field can be customized by the Enterprise.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Reference3" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="REFERENCE3" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Additional reference information. This field can be customized by the Enterprise.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Tax" type="yfctype:PlusMinusMoney" use="optional">
                <xsd:annotation yfc:columnName="TAX" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>
                        <div>
								The tax amount. <code>Tax</code> cannot be less than zero.  During the default repricing logic, this is recalculated using the <code>TaxPercentage</code> and the price of the related <code>ChargeCategory</code>.  This can be overridden with the YFSRecalulateLineTaxUE.
		                            </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxName" type="yfctype:Text-40" use="required">
                <xsd:annotation yfc:columnName="TAX_NAME" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>The tax name.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxPercentage" type="yfctype:TaxType" use="optional">
                <xsd:annotation yfc:columnName="TAX_PERCENTAGE" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Percentage amount for tax calculation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxableFlag" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="TAXABLE_FLAG" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>This flag indicates if a charge can be taxed.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="KitLines">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:KitLine"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="KitLine">
        <xsd:annotation yfc:tableName="YFS_ORDER_KIT_LINE">
            <xsd:documentation>This table stores information about kitted component items. A kit is a collection of items that form a unit. For example, a home theatre system is a unit consisting of component items such as a DVD player, TV, CD player, etc.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:KitLineTranQuantity"/>
            </xsd:all>
            <xsd:attribute name="CustomerItem" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="CUSTOMER_ITEM" yfc:tableName="YFS_ORDER_KIT_LINE">
                    <xsd:documentation>Cross reference to the customer item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CustomerItemDesc" type="yfctype:Text-500" use="optional">
                <xsd:annotation yfc:columnName="CUSTOMER_ITEM_DESCRIPTION" yfc:tableName="YFS_ORDER_KIT_LINE">
                    <xsd:documentation>Customer item description.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DepartmentCode" type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="DEPARTMENT_CODE" yfc:tableName="YFS_ORDER_KIT_LINE">
                    <xsd:documentation>Department that deals with this item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemDesc" type="yfctype:Text-500" use="optional">
                <xsd:annotation yfc:columnName="ITEM_DESCRIPTION" yfc:tableName="YFS_ORDER_KIT_LINE">
                    <xsd:documentation>Description for the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemID" type="yfctype:ItemID" use="required">
                <xsd:annotation yfc:columnName="ITEM_ID" yfc:tableName="YFS_ORDER_KIT_LINE">
                    <xsd:documentation>The unique identifer for the component item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemShortDesc" type="yfctype:Text-200" use="optional">
                <xsd:annotation yfc:columnName="ITEM_SHORT_DESCRIPTION" yfc:tableName="YFS_ORDER_KIT_LINE">
                    <xsd:documentation>short description for the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemWeight" type="yfctype:Dimension" use="optional">
                <xsd:annotation yfc:columnName="ITEM_WEIGHT" yfc:tableName="YFS_ORDER_KIT_LINE">
                    <xsd:documentation>Weight of the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemWeightUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="ITEM_WEIGHT_UOM" yfc:tableName="YFS_ORDER_KIT_LINE">
                    <xsd:documentation>Unit of measure of the ITEM_WEIGHT attribute.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="KitQty" type="yfctype:Quantity" use="required">
                <xsd:annotation yfc:columnName="KIT_QTY" yfc:tableName="YFS_ORDER_KIT_LINE">
                    <xsd:documentation>The number of components required to one kit.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ProductClass" type="yfctype:ProductClass" use="optional">
                <xsd:annotation yfc:columnName="PRODUCT_CLASS" yfc:tableName="YFS_ORDER_KIT_LINE">
                    <xsd:documentation>Component Product Class.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SupplierItem" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="SUPPLIER_ITEM" yfc:tableName="YFS_ORDER_KIT_LINE">
                    <xsd:documentation>Cross reference to the supplier ietm.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SupplierItemDesc" type="yfctype:ItemDesc" use="optional">
                <xsd:annotation yfc:columnName="SUPPLIER_ITEM_DESCRIPTION" yfc:tableName="YFS_ORDER_KIT_LINE">
                    <xsd:documentation>Supplier item description.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UPCCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="UPC_CODE" yfc:tableName="YFS_ORDER_KIT_LINE">
                    <xsd:documentation>UPC code for the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitCost" type="yfctype:UnitPrice" use="optional">
                <xsd:annotation yfc:columnName="UNIT_COST" yfc:tableName="YFS_ORDER_KIT_LINE">
                    <xsd:documentation>Unit cost of the component item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitOfMeasure" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="UOM" yfc:tableName="YFS_ORDER_KIT_LINE">
                    <xsd:documentation>Component Unit Of Measure.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="KitLineTranQuantity">
        <xsd:annotation>
            <xsd:documentation>
					   Contains information for any <code>OrderLine</code> item that is part of a Kit. This occurs when multiple SKUs are grouped to make up one item.
					   </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="KitQty" type="yfctype:Quantity" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
							The number of components required to create one kit.
							<Validation>
                                <Condition> Any non-zero positive number.
								</Condition>
                                <Error Description="java.lang.NumberFormatException" ErrorCode=""/>
                            </Validation>
                            <Validation>
                                <Condition>Must be passed if the <code>KitLine</code> is passed.
								</Condition>
                                <Error Description="Zero Kit Quantity." ErrorCode="YFS10382"/>
                                <Condition>Number passed has a fraction.
								</Condition>
                                <Error Description="Fractional quantity can not be passed to this attribute." ErrorCode="YFS10447"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TransactionalUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
							  UOM on which all the transactional quantities are specified.
							  </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Schedules">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:Schedule"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Schedule">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE_SCHEDULE">
            <xsd:documentation>
                <div>
					Contains Order Line Schedule information for Order Lines. An order line can be broken up into multiple schedules. Each Schedule contains information about ShipNode, ReceivingNode, ExpectedShipmentDate, ExpectedDeliveryDate and info about any tags associated with the order line.
					<Validation>
                        <Condition>If the item on the line is not tag controlled and <code>BatchNo</code>, <code>LotNumber</code>, or <code>RevisionNo</code> is passed.
						 </Condition>
                        <Error Description="Item is not tag controlled." ErrorCode="OMP11032"/>
                    </Validation>
                </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ScheduleTranQuantity"/>
            </xsd:all>
            <xsd:attribute name="BatchNo" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="BATCH_NO" yfc:tableName="YFS_ORDER_LINE_SCHEDULE">
                    <xsd:documentation>The batch number that has been specified on the scheduled order line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ChangeInQuantity" type="yfctype:PositiveQuantity" use="required">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
					The quantity change to be applied.
					<br/>Valid Values:Non-zero, positive integer.
					<br/> The sum of all schedules' <code>Quantity</code> cannot exceed <code>OrderedQty</code> in <code>OrderLine</code>.
						If the <code>OrderedQty</code> is greater than the total schedule quantity which remains, then <code>Quantity</code> is created with no schedule information.
					<Validation>
                                <Condition>Must be passed if <code>Schedule is passed</code>.
						</Condition>
                                <Error Description="Invalid Schedule Quantity." ErrorCode="Error YFS10377"/>
                            </Validation>
                            <Validation>
                                <Condition>If total schedule quantity is greater than <code>OrderedQty</code>
                                </Condition>
                                <Error Description="Invalid Schedule Quantity." ErrorCode="YFS10377"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LotNumber" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="LOT_NUMBER" yfc:tableName="YFS_ORDER_LINE_SCHEDULE">
                    <xsd:documentation>The lot number that has been specified on the scheduled order line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RevisionNo" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="REVISION_NO" yfc:tableName="YFS_ORDER_LINE_SCHEDULE">
                    <xsd:documentation>The revision number that has been specified on the scheduled order line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ScheduleTranQuantity">
        <xsd:complexType>
            <xsd:attribute name="ChangeInQuantity" type="yfctype:Quantity" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
							Quantity.
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TransactionalUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
						  UOM on which all the transactional quantities are specified.
						  </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Dependency">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE">
            <xsd:documentation>
                <div>
				 This element is  present only if the <code>OrderLine</code> depends on another <code>OrderLine</code>.
				</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="DependentOnPrimeLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:isConditional="Y">
                    <xsd:documentation>
                        <div>
					Both <code>DependentOnPrimeLineNo</code> and <code>DependentOnSubLineNo</code> must be passed for dependent <code>OrderLines</code>.
					<Validation>
                                <Condition>The <code>DependentOnPrimeLineNo</code> and <code>DependentOnSubLineNo</code> combination passed does not match any other <code>OrderLine</code>.
						</Condition>
                                <Error Description="Invalid dependent on line information passed" ErrorCode="YFS10444"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DependentOnSubLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:isConditional="Y">
                    <xsd:documentation>
                        <div>
					Both <code>DependentOnPrimeLineNo</code> and <code>DependentOnSubLineNo</code> must be passed for dependent <code>OrderLine</code>s.
					<Validation>
                                <Condition>The <code>DependentOnPrimeLineNo</code> and <code>DependentOnSubLineNo</code> combination passed does not match any other <code>OrderLine</code>.
						</Condition>
                                <Error Description="Invalid dependent on line information passed" ErrorCode="YFS10444"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DependentOnTransactionalLineId" type="yfctype:Text-40" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
					This is a virtual field meant to identify <code>OrderLine</code> when <code>DependentOnPrimeLineNo</code> and <code>DependentOnSubLineNo</code> is not passed. This field is is not persistent in the database.
					<Validation>
                                <Condition> The <code>DependentOnTransactionalLineId</code> passed does not match any <code>OrderLine</code>.
						</Condition>
                                <Error Description="Invalid dependent on line information passed" ErrorCode="YFS10444"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderLineInvAttRequest">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE_REQ_TAG">
            <xsd:documentation>
                <div>
			 This element does not expose a quantity field. The definition provided applies to the entire line quantity, minus the quantity for which specific tag information is passed in the <code>Schedule </code> element. The requested tag attributes passed in the input XML do not necessarily have to be all the unique attributes unlike the <code>Schedule</code> element under <code>OrderLine</code>. No error is thrown if tag attributes passed do not result in a specific tag number. If the tag attributes passed results in determining a unique tag number, then the tag number, LotNo, BatchNo and RevisionNo are set in the YFS_ORDER_LINE_SCHEDULE table. If multiple attributes define the tag as unique (and defined in YFS_ITEM_TAG), then the <code>getTagNo</code> user exit is called.
			 <Validation>
                        <Condition>If the item on the line is not tag controlled, and the <code>OrderLineInvAttRequest</code> element is passed.
				 </Condition>
                        <Error Description="Item is not tag controlled." ErrorCode="OMP11032"/>
                    </Validation>
                </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="BatchNo" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="BATCH_NO" yfc:tableName="YFS_ORDER_LINE_REQ_TAG">
                    <xsd:documentation>The Batch Number that has been specifed in the Order Line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LotAttribute1" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="LOT_ATTRIBUTE1" yfc:tableName="YFS_ORDER_LINE_REQ_TAG">
                    <xsd:documentation>Lot Attribute 1 used to specify the details of the lot.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LotAttribute2" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="LOT_ATTRIBUTE2" yfc:tableName="YFS_ORDER_LINE_REQ_TAG">
                    <xsd:documentation>Lot Attribute 2 used to specify the details of the lot.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LotAttribute3" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="LOT_ATTRIBUTE3" yfc:tableName="YFS_ORDER_LINE_REQ_TAG">
                    <xsd:documentation>Lot Attribute 3 used to specify the details of the lot.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LotKeyReference" type="yfctype:Text-4000" use="optional">
                <xsd:annotation yfc:columnName="LOT_KEY_REFERENCE" yfc:tableName="YFS_ORDER_LINE_REQ_TAG">
                    <xsd:documentation>The Lot key reference that has been specifed in the Order Line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LotNumber" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="LOT_NUMBER" yfc:tableName="YFS_ORDER_LINE_REQ_TAG">
                    <xsd:documentation>The Lot Number that has been specifed in the Order Line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ManufacturingDate" use="optional">
                <xsd:annotation yfc:columnName="LOT_MANUFACTURE_DATE" yfc:tableName="YFS_ORDER_LINE_REQ_TAG">
                    <xsd:documentation>The Lot manufacturing date that has been specifed in the Order Line.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Date"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="RevisionNo" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="REVISION_NO" yfc:tableName="YFS_ORDER_LINE_REQ_TAG">
                    <xsd:documentation>The Revision Number that has been specifed in the Order Line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ProductItems">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ProductItem"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ProductItem">
        <xsd:annotation yfc:tableName="YFS_ORDER_SER_PROD_ITEM">
            <xsd:documentation>
                <div>
                    <code>ProductItem</code> contains the definition of product items involved in a stand-alone service line. This definition is used to calculate the service line quantity and to identify the skills needed to provide the service. These product items will not be fulfilled as part of the order fulfillment process.
				<Validation>
                        <Condition> Product item definition can be given only for a stand-alone provided service <code>OrderLine</code>.
					</Condition>
                        <Error Description="Product item definition can be given only for Standalone Provided Services" ErrorCode="OMP11048_55SP2"/>
                    </Validation>
                    <Validation>
                        <Condition>The <code>ItemID</code>s or product item and service line must have an association present.
					</Condition>
                        <Error Description="Missing association between Product and Service Line Items" ErrorCode="OMP10028"/>
                    </Validation>
                </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ItemID" type="yfctype:ItemID" use="optional">
                <xsd:annotation yfc:columnName="ITEM_ID" yfc:tableName="YFS_ORDER_SER_PROD_ITEM">
                    <xsd:documentation>The unique identifier for the Product Item. The item ID, UOM and OrganizationCode form the unique item identifier for Product Item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Quantity" type="yfctype:Quantity" use="optional">
                <xsd:annotation yfc:columnName="QUANTITY" yfc:tableName="YFS_ORDER_SER_PROD_ITEM">
                    <xsd:documentation>Product item quantity.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitOfMeasure" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="UOM" yfc:tableName="YFS_ORDER_SER_PROD_ITEM">
                    <xsd:documentation>Unit of measure of the product item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderLineReservations">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:OrderLineReservation"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderLineReservation">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE_RESERVATION">
            <xsd:documentation>
                <div>Identifies a reservation to make for the order line.  Existing reservations cannot be modified directly; instead, if a new reservation is created with the same reservation attributes as an existing reservation, the new reservation will be consolidated into the existing reservation, and demand will be created for the new quantity.<br/>If new quantity is being added to the order line in the same call that a reservation is being added, and the reservation matches the order line's attributes, then the reservation will be rolled up in to the order line, and the matching quantity will move to "Reserved" status.</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="BatchNo" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="BATCH_NO" yfc:tableName="YFS_ORDER_LINE_RESERVATION">
                    <xsd:documentation>The batch number that has been specified on the reservation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DemandType" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="DEMAND_TYPE" yfc:tableName="YFS_ORDER_LINE_RESERVATION">
                    <xsd:documentation>The inventory demand type used by the reservation.  For all user-created reservations, this will be defaulted from the document params.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemID" type="yfctype:ItemID" use="required">
                <xsd:annotation yfc:columnName="ITEM_ID" yfc:tableName="YFS_ORDER_LINE_RESERVATION">
                    <xsd:documentation>The reserved item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LotNumber" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="LOT_NUMBER" yfc:tableName="YFS_ORDER_LINE_RESERVATION">
                    <xsd:documentation>The lot number that has been specified on the reservation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Node" type="yfctype:Key" use="required">
                <xsd:annotation yfc:columnName="NODE_KEY" yfc:tableName="YFS_ORDER_LINE_RESERVATION">
                    <xsd:documentation>The ship node at which the item has been reserved.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ProductClass" type="yfctype:ProductClass" use="optional">
                <xsd:annotation yfc:columnName="PRODUCT_CLASS" yfc:tableName="YFS_ORDER_LINE_RESERVATION">
                    <xsd:documentation>The reserved item's product class.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Quantity" type="yfctype:Quantity" use="required">
                <xsd:annotation yfc:columnName="QUANTITY" yfc:tableName="YFS_ORDER_LINE_RESERVATION">
                    <xsd:documentation>
                        <div>The quantity to reserve.</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RequestedReservationDate" type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="REQ_RESERVATION_DATE" yfc:tableName="YFS_ORDER_LINE_RESERVATION">
                    <xsd:documentation>The date when the reserved inventory is needed, such as the shipping date.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReservationID" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="RESERVATION_ID" yfc:tableName="YFS_INVENTORY_RESERVATION">
                    <xsd:documentation>
                        <div>The reservation ID from an existing inventory reservation.  If the reservation exists, that quantity will be consumed first.  Any quantity that cannot be obtained from the reservation will be reserved on the fly.  If <code>ReservationID</code> is not passed, all quantity for this reservation will be reserved on the fly.</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RevisionNo" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="REVISION_NO" yfc:tableName="YFS_ORDER_LINE_RESERVATION">
                    <xsd:documentation>The revision number that has been specified on the reservation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitOfMeasure" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="UOM" yfc:tableName="YFS_ORDER_LINE_RESERVATION">
                    <xsd:documentation>The reserved item's unit of measure.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="BundleParentLine">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE">
            <xsd:documentation>
					Identifies the bundle parent order line for this line.
					<Validation>
                    <Condition>
                        <code>OrderLineKey</code>
                        <i>or</i>
                        <code>PrimeLineNo</code> and <code>SubLineNo</code>
                        <i>or</i>
                        <code>TransactionalLineId</code> must be passed, and must identify a valid order line.
						</Condition>
                    <Error Description="Invalid order line" ErrorCode="OMP10009"/>
                </Validation>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="OrderLineKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORDER_LINE_KEY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
					Primary key generated by the system. May be passed to identify the <code>OrderLine</code>. Alternatively, the <code>TransactionalLineId</code>
                            <i>or</i>
                            <br/>the <code>PrimeLineNo</code> and the <code>SubLineNo</code> can be passed.

					</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PrimeLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="PRIME_LINE_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
					The order line number. Must be passed with the <code>SubLineNo</code> to identify the <code>OrderLine</code>. Alternatively, the <code>TransactionalLineId</code>
                            <i>or</i> the <code>OrderLineKey</code> can be passed.
						</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SubLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="SUB_LINE_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
					Must be passed with the <code>PrimeLineNo</code> to identify the <code>OrderLine</code>. Alternatively, the <code>TransactionalLineId</code>
                            <i>or</i> the <code>OrderLineKey</code> can be passed.
					</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TransactionalLineId" type="yfctype:Text-40" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
					A virtual field meant to identify order lines when <code>PrimeLineNo</code> and <code>SubLineNo</code> are not passed. Used in Line Associations. This value is not stored in the database. While creating a new line and associating it with another line, the same attribute must be passed in the <code>OrderLine</code> element and in this element to uniquely identify a line if <code>OrderLineKey</code> or <code>PrimeLineNo</code> and <code>SubLineNo</code> are not present.
					</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderOverride">
        <xsd:annotation yfc:tableName="YFS_ORDER_OVERRIDE">
            <xsd:documentation>This table stores a set of flags indicating whether an order attribute has been overridden.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="QuantityLimitOverridden" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="QUANTITY_LIMIT_OVERRIDDEN" yfc:tableName="YFS_ORDER_OVERRIDE">
                    <xsd:documentation>This flag indicates whether the order line's min/max quantity limits have been overridden. Valid values: 'Y' or 'N'.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ValidateItem" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="VALIDATE_ITEM" yfc:tableName="YFS_ORDER_OVERRIDE">
                    <xsd:documentation>This flag indicates whether item validation is enabled. Valid Values: Y or N. Default: Y</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ProductServiceAssocs">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:ProductServiceAssoc"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ProductServiceAssoc">
        <xsd:annotation yfc:tableName="YFS_ORDER_PROD_SER_ASSOC">
            <xsd:documentation>
                <div>
                    <code>ProductServiceAssoc</code> contains the <code>ProductLine</code> and <code>ServiceLine</code> elements which identify the Product and Service line being associated. Both <code>ProductLine</code> and <code>ServiceLine</code> elements must be passed to uniquely identify the lines. If multiple associations need to be made, multiple <code>ProductServiceAssoc</code> nodes should be passed. For a bundle, either associate bundle parent or bundle components with service line not both. Refer to <code>ProductLine</code> and <code>ServiceLine</code> elements for further details.
			</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ProductLine"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ServiceLine"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ProductLine">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE">
            <xsd:documentation>
                <div>
				Specifies the product and service lines being associated. When a new association is created by passing the <code>ProductServiceAssoc</code>, a number of validations are performed.
				<Validation>
                        <Condition> Only a delivery or provided service <code>OrderLine</code> can be associated with a product <code>OrderLine</code>
                        </Condition>
                        <Error Description="Invalid Service Line passed in Association" ErrorCode="OMP10024"/>
                    </Validation>
                    <Validation>
                        <Condition>A delivery <code>OrderLine</code>or a service  <code>OrderLine</code> which is not standalone must be associated with a product  <code>OrderLine</code>.
					</Condition>
                        <Error Description="Service Line does not have any Association with Product Line" ErrorCode="OMP10022:"/>
                    </Validation>
                    <Validation>
                        <Condition>One product  <code>OrderLine</code> cannot be associated with more than one delivery  <code>OrderLine</code>.
					</Condition>
                        <Error Description="One Product Line cannot be associated with Multiple Delivery Lines" ErrorCode="OMP10027"/>
                    </Validation>
                    <Validation>
                        <Condition>The <code>ItemID</code>s or product line and service line must have an association present.
					</Condition>
                        <Error Description="Missing association between Product and Service Line Items" ErrorCode="OMP10028"/>
                    </Validation>
                    <Validation>
                        <Condition>A stand-alone provided service <code>OrderLine</code> can not associated with any product  <code>OrderLine</code>.
					</Condition>
                        <Error Description=" A Standalone Service line cannot be associated with Product Lines" ErrorCode="OMP10054"/>
                    </Validation>
                    <br/>If multiple product <code>OrderLine</code>s are associated with one delivery <code>OrderLine</code>, there must be consistent information related to delivery. If the Delivery line <b>does not</b> have values for the following attributes in <code>OrderLine</code> element : <code>PersonInfoShipTo</code>, <code>ReqShipDate</code>,  <code>ReqDeliveryDate</code>,  <code>MergeNode</code>, <code>ReceivingNode</code>,  <code>ShipNode</code>
                    <br/>
				the product OrderLines associated must not have <b>differing</b> values for any of those elements and attributes. e.g. If a Delivery line is attached to two different product lines and if <code>ReqShipDate</code> is not present on Delivery line , then if the product lines have different values for <code>ReqShipDate</code>, an error will be thrown by the api.
				<br/>
                    <br/>If the delivery <code>OrderLine</code> has any of these attributes present, the value is copied to all associated product <code>OrderLine</code>s,  even if different values already exist for the Product  <code>OrderLine</code>s. If Product line has these values but Delivery does not have these values, then these values are copied over to Delivery orderline also. <code>EarliestShipDate</code> and <code>EarliestDeliveryDate</code> are computed by the api and are also synched between Delivery and Product <code>OrderLine</code>s.
			</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="OrderLineKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORDER_LINE_KEY" yfc:isConditional="Y" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
				Primary key generated by the system. May be passed to identify the <code>OrderLine</code>. Alternatively, the <code>TransactionalLineId</code>
                            <i>or</i>
                            <br/>the <code>PrimeLineNo</code> and the <code>SubLineNo</code> can be passed.
				<Validation>
                                <Condition>
                                    <code>OrderLineKey</code> must identify an existing OrderLine.
					</Condition>
                                <Error Description="Invalid Product Line passed in Association" ErrorCode="OMP10023"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PrimeLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="PRIME_LINE_NO" yfc:isConditional="Y" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
				The order line number. Must be passed with the <code>SubLineNo</code> to identify the <code>OrderLine</code>. Alternatively, the <code>TransactionalLineId</code>
                            <i>or</i> the <code>OrderLineKey</code> can be passed.
				<Validation>
                                <Condition>Must pass one of the following:
					<br/>the <code>PrimeLineNo</code> and the <code>SubLineNo</code>
                                    <br/>
                                    <i>or</i>
                                    <code>TransactionalLineId</code>
                                    <i>or</i> the <code>OrderLineKey</code>.
					</Condition>
                                <Error Description="" ErrorCode=""/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SubLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="SUB_LINE_NO" yfc:isConditional="Y" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
				Must be passed with the <code>PrimeLineNo</code> to identify the <code>OrderLine</code>. Alternatively, the <code>TransactionalLineId</code>
                            <i>or</i> the <code>OrderLineKey</code> can be passed.
				<Validation>
                                <Condition>Must pass one of the following:
					<br/>the <code>PrimeLineNo</code> and the <code>SubLineNo</code>
                                    <br/>
                                    <i>or</i>
                                    <code>TransactionalLineId</code>
                                    <i>or</i> the <code>OrderLineKey</code>.
					</Condition>
                                <Error Description="" ErrorCode=" "/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TransactionalLineId" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:isConditional="Y">
                    <xsd:documentation>
                        <div>
				A virtual field meant to identify order lines when <code>PrimeLineNo</code> and <code>SubLineNo</code> are not passed. Used in Line Associations. This value is not stored in the database. While creating a new line and associating it with another line, the same attribute must be passed in the <code>OrderLine</code> element and in this element to uniquely identify a line if <code>OrderLineKey</code> or <code>PrimeLineNo</code> and <code>SubLineNo</code> are not present.
				<Validation>
                                <Condition>Must pass one of the following:
					<br/>the <code>PrimeLineNo</code> and the <code>SubLineNo</code>
                                    <br/>
                                    <i>or</i>
                                    <code>TransactionalLineId</code>
                                    <i>or</i> the <code>OrderLineKey</code>.
					</Condition>
                                <Error Description="" ErrorCode=" "/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ServiceLine">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE">
            <xsd:documentation>This table stores order line information, including item details and shipping information. Multiple lines can be created for a single order defined in the YFS_ORDER_HEADER table.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="OrderLineKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORDER_LINE_KEY" yfc:isConditional="Y" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
				Primary key generated by the system.
				<Validation>
                                <Condition>
                                    <code>OrderLineKey</code> must identify an existing <code>OrderLine</code>.
					</Condition>
                                <Error Description="Invalid Service Line passed in Association" ErrorCode="OMP10024"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PrimeLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="PRIME_LINE_NO" yfc:isConditional="Y" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
				The order line number. Part of an order line's unique key.
				<br/>Must pass one of the following :
					<br/>The <code>PrimeLineNo</code>  and the <code>SubLineNo</code>
                            <i>or</i>
                            <code>OrderLineKey</code>
                            <i>or</i>
                            <code>TransactionalLineId</code>.
				</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SubLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="SUB_LINE_NO" yfc:isConditional="Y" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
				Part of unique key of line.
					<br/>Must pass one of the following :
					<br/>The <code>PrimeLineNo</code>  and the <code>SubLineNo</code>
                            <i>or</i>
                            <code>OrderLineKey</code>
                            <i>or</i>
                            <code>TransactionalLineId</code>.
				</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TransactionalLineId" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:isConditional="Y">
                    <xsd:documentation>
                        <div>
				A virtual field meant to identify order lines when <code>PrimeLineNo</code> and <code>SubLineNo</code> are not passed. Used in Line Associations. This value is not stored in the database. While creating a new line and associating it with another line, the same attribute must be passed in the <code>OrderLine</code> element as well in this element to uniquely identify a line if <code>OrderLineKey</code> or <code>PrimeLineNo</code> and <code>SubLineNo</code> are not present.
					<br/>Must pass one of the following :
					<br/>The <code>PrimeLineNo</code>  and the <code>SubLineNo</code>
                            <i>or</i>
                            <code>OrderLineKey</code>
                            <i>or</i>
                            <code>TransactionalLineId</code>.
				</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderLineRelationships">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:OrderLineRelationship"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderLineRelationship">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE_RELATIONSHIP">
            <xsd:documentation>
                <div>
                    <code>OrderLineRelationship</code> contains the <code>ParentLine</code> and <code>ChildLine</code> elements which identify the Parent and Child line. Both <code>ParentLine</code> and <code>ChildLine</code> elements must be passed to uniquely identify the lines. If multiple OrderLineRelationships need to be made, multiple <code>OrderLineRelationship</code> nodes should be passed. Refer to <code>ParentLine</code> and <code>ChildLine</code> elements for further details.
			<Validation>
                        <Condition> If circular dependency is detected for order lines of the same <code>RelationshipType</code>. The following error is displayed.
					</Condition>
                        <Error Description="Circular dependency exists." ErrorCode="YFS10446"/>
                    </Validation>
                </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ParentLine"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ChildLine"/>
            </xsd:all>
            <xsd:attribute name="RelationshipType" type="yfctype:Text-40" use="required">
                <xsd:annotation yfc:columnName="RELATIONSHIP_TYPE" yfc:tableName="YFS_ORDER_LINE_RELATIONSHIP">
                    <xsd:documentation>
                        <div>
						The type of relationship between the parent and the child line. There is no validation for the <code>RelationshipType</code> attribute.
						</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ParentLine">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE">
            <xsd:documentation>
                <div>
				Specifies the parent and the child line being related. When a new relationship is created by passing the <code>OrderLineRelationship</code> the following validation is performed.
				<Validation>
                        <Condition> If either the parent line or the child line is not passed with the appropriate line identifiers the following error is displayed.
					</Condition>
                        <Error Description="Invalid Line Passed in Relationship" ErrorCode="OMP79_0024"/>
                    </Validation>
                </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="OrderLineKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORDER_LINE_KEY" yfc:isConditional="Y" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
				Primary key generated by the system. May be passed to identify the <code>OrderLine</code>. Alternatively, the <code>TransactionalLineId</code>
                            <i>or</i>
                            <br/>the <code>PrimeLineNo</code> and the <code>SubLineNo</code> can be passed.
				<Validation>
                                <Condition>
                                    <code>OrderLineKey</code> must identify an existing OrderLine.
					</Condition>
                                <Error Description="Invalid Line Passed in Relationship" ErrorCode="OMP79_0024"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PrimeLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="PRIME_LINE_NO" yfc:isConditional="Y" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
				The order line number. Must be passed with the <code>SubLineNo</code> to identify the <code>OrderLine</code>. Alternatively, the <code>TransactionalLineId</code>
                            <i>or</i> the <code>OrderLineKey</code> can be passed.
				<Validation>
                                <Condition>Must pass one of the following:
					<br/>the <code>PrimeLineNo</code> and the <code>SubLineNo</code>
                                    <br/>
                                    <i>or</i>
                                    <code>TransactionalLineId</code>
                                    <i>or</i> the <code>OrderLineKey</code>.
					</Condition>
                                <Error Description="" ErrorCode=""/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SubLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="SUB_LINE_NO" yfc:isConditional="Y" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
				Must be passed with the <code>PrimeLineNo</code> to identify the <code>OrderLine</code>. Alternatively, the <code>TransactionalLineId</code>
                            <i>or</i> the <code>OrderLineKey</code> can be passed.
				<Validation>
                                <Condition>Must pass one of the following:
					<br/>the <code>PrimeLineNo</code> and the <code>SubLineNo</code>
                                    <br/>
                                    <i>or</i>
                                    <code>TransactionalLineId</code>
                                    <i>or</i> the <code>OrderLineKey</code>.
					</Condition>
                                <Error Description="" ErrorCode=" "/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TransactionalLineId" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:isConditional="Y">
                    <xsd:documentation>
                        <div>
				A virtual field meant to identify order lines when <code>PrimeLineNo</code> and <code>SubLineNo</code> are not passed. Used in Line Associations. This value is not stored in the database. While creating a new line and associating it with another line, the same attribute must be passed in the <code>OrderLine</code> element and in this element to uniquely identify a line if <code>OrderLineKey</code> or <code>PrimeLineNo</code> and <code>SubLineNo</code> are not present.
				<Validation>
                                <Condition>Must pass one of the following:
					<br/>the <code>PrimeLineNo</code> and the <code>SubLineNo</code>
                                    <br/>
                                    <i>or</i>
                                    <code>TransactionalLineId</code>
                                    <i>or</i> the <code>OrderLineKey</code>.
					</Condition>
                                <Error Description="" ErrorCode=" "/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ChildLine">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE">
            <xsd:documentation>This table stores order line information, including item details and shipping information. Multiple lines can be created for a single order defined in the YFS_ORDER_HEADER table.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="OrderLineKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORDER_LINE_KEY" yfc:isConditional="Y" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
				Primary key generated by the system.
				<Validation>
                                <Condition>
                                    <code>OrderLineKey</code> must identify an existing <code>OrderLine</code>.
					</Condition>
                                <Error Description="Invalid Line Passed in Relationship" ErrorCode="OMP79_0024"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PrimeLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="PRIME_LINE_NO" yfc:isConditional="Y" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
				The order line number. Part of an order line's unique key.
				<br/>Must pass one of the following :
					<br/>The <code>PrimeLineNo</code>  and the <code>SubLineNo</code>
                            <i>or</i>
                            <code>OrderLineKey</code>
                            <i>or</i>
                            <code>TransactionalLineId</code>.
				</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SubLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="SUB_LINE_NO" yfc:isConditional="Y" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
				Part of unique key of line.
					<br/>Must pass one of the following :
					<br/>The <code>PrimeLineNo</code>  and the <code>SubLineNo</code>
                            <i>or</i>
                            <code>OrderLineKey</code>
                            <i>or</i>
                            <code>TransactionalLineId</code>.
				</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TransactionalLineId" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:isConditional="Y">
                    <xsd:documentation>
                        <div>
				A virtual field meant to identify order lines when <code>PrimeLineNo</code> and <code>SubLineNo</code> are not passed. Used in Line Associations. This value is not stored in the database. While creating a new line and associating it with another line, the same attribute must be passed in the <code>OrderLine</code> element as well in this element to uniquely identify a line if <code>OrderLineKey</code> or <code>PrimeLineNo</code> and <code>SubLineNo</code> are not present.
					<br/>Must pass one of the following :
					<br/>The <code>PrimeLineNo</code>  and the <code>SubLineNo</code>
                            <i>or</i>
                            <code>OrderLineKey</code>
                            <i>or</i>
                            <code>TransactionalLineId</code>.
				</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PersonInfoContact">
        <xsd:annotation yfc:tableName="YFS_PERSON_INFO">
            <xsd:documentation>This table stores relevant personal information and addresses. Addresses are typically organization address, ship node address, customer address, and so forth. Personal information is required in cases where person to person contact needs to be established; for example, taking appointments or speaking during negotiations. The key of this table is posted in various tables to allow access to people and address information.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="AddressID" type="yfctype:Text-256" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_ID" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>A user specified label for an address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine1" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE1" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 1 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine2" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE2" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 2 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine3" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE3" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 3 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine4" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE4" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 4 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine5" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE5" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 5 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine6" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE6" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 6 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AlternateEmailID" type="yfctype:EMailID" use="optional">
                <xsd:annotation yfc:columnName="ALTERNATE_EMAILID" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The alternate email address at which the person or a predesignated employee of the organization referred to in the address can be contacted.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Beeper" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="BEEPER" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The beeper number at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="City" type="yfctype:City" use="optional">
                <xsd:annotation yfc:columnName="CITY" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The city in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Company" type="yfctype:Company" use="optional">
                <xsd:annotation yfc:columnName="COMPANY" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The company this person is associated with.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Country" type="yfctype:Country" use="optional">
                <xsd:annotation yfc:columnName="COUNTRY" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The country in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DayFaxNo" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="DAY_FAX_NO" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The day time fax at which the person or a predesignated employee of the organization referred to in the address can be notified.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DayPhone" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="DAY_PHONE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The day time phone at which the person or organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Department" type="yfctype:Department" use="optional">
                <xsd:annotation yfc:columnName="DEPARTMENT" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The primary department in charge of procurement.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EMailID" type="yfctype:EMailID" use="optional">
                <xsd:annotation yfc:columnName="EMAILID" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The email address at which the person or a predesignated employee of the organization referred to in the address can be contacted.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EveningFaxNo" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="EVENING_FAX_NO" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The evening time fax at which the person or a predesignated employee of the organization referred to in the address can be notified.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EveningPhone" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="EVENING_PHONE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The evening time phone at which the person or organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FirstName" type="yfctype:PersonName" use="optional">
                <xsd:annotation yfc:columnName="FIRST_NAME" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The first name of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsCommercialAddress" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_COMMERCIAL_ADDRESS" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>Indicates whether the address is commercial, residential or none. Values of this field can be Y -Commercial address.N - Residential address. undefined for Null.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="JobTitle" type="yfctype:Title-40" use="optional">
                <xsd:annotation yfc:columnName="JOB_TITLE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The job title of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LastName" type="yfctype:PersonName" use="optional">
                <xsd:annotation yfc:columnName="LAST_NAME" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The last name of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MiddleName" type="yfctype:MiddleName" use="optional">
                <xsd:annotation yfc:columnName="MIDDLE_NAME" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The middle name of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MobilePhone" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="MOBILE_PHONE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The mobile phone at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OtherPhone" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="OTHER_PHONE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The alternate phone number at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PersonID" type="yfctype:PersonId" use="optional">
                <xsd:annotation yfc:columnName="PERSON_ID" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The unique identifier for a person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="State" type="yfctype:State" use="optional">
                <xsd:annotation yfc:columnName="STATE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The state in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Suffix" type="yfctype:Suffix" use="optional">
                <xsd:annotation yfc:columnName="SUFFIX" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The suffix for a person, such as Jr., or Sr.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxGeoCode" type="yfctype:GeoCode" use="optional">
                <xsd:annotation yfc:columnName="TAX_GEO_CODE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The tax geo code for this location. Intended for future use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Title" type="yfctype:Title-10" use="optional">
                <xsd:annotation yfc:columnName="TITLE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The title for the person such as Ms., Mr., or Dr..</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ZipCode" type="yfctype:ZipCode" use="optional">
                <xsd:annotation yfc:columnName="ZIP_CODE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The zip code in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PriceInfo">
        <xsd:annotation yfc:tableName="YFS_ORDER_HEADER">
            <xsd:documentation>This table stores all order documents in the system.  These orders can be further classified into various types of documents. For example, ORDERS, PLANNED ORDERS and any other future types that the system may provide.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Currency" type="yfctype:Currency" use="optional">
                <xsd:annotation yfc:columnName="CURRENCY" yfc:tableName="YFS_ORDER_HEADER">
                    <xsd:documentation>
                        <div>
			The currency code in which this order is being processed. All price fields are assumed to be in this currency.
			<br/>Default: Currency that is the default for the Enterprise.
			<Validation>
                                <Condition>Must be valid currency for the Enterprise's ruleset, in the table <code>YFS_CURRENCY</code>.
				</Condition>
                                <Error Description="Invalid Currency." ErrorCode="YFS10325"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EnterpriseCurrency" type="yfctype:Currency" use="optional">
                <xsd:annotation yfc:columnName="ENTERPRISE_CURRENCY" yfc:tableName="YFS_ORDER_HEADER">
                    <xsd:documentation>
                        <div>
			If the <code>EnterpriseCurrency</code> value is not passed, the Enterprise's locale is used to get a currency for that locale.
			The currency for the locale is validated for the Enterprise's ruleset. If no errors occur during lookup and validation, the <code>EnterpriseCurrency</code> is defaulted to the currency for that locale.
			<Validation>
                                <Condition>If the <code>EnterpriseCurrency</code> value is passed, or is calculated based on enterprise locale, it must be a valid currency for the Enterprise's ruleset, in the table <code>YFS_CURRENCY</code>.
				</Condition>
                                <Error Description="Invalid Currency." ErrorCode="YFS10325"/>
                            </Validation>
                            <Validation>
                                <Condition>If the <code>EnterpriseCurrency</code> value is not passed, and the Enterprise's locale is invalid
				</Condition>
                                <Error Description="" ErrorCode="YFS10330"/>
                                <Condition>If the <code>EnterpriseCurrency</code> value is not passed, and no locale is set for the  Enterprise's ruleset.
				</Condition>
                                <Error Description="Invalid Currency." ErrorCode="YFS10325"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReportingConversionDate" type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="REPORTING_CONVERSION_DATE" yfc:tableName="YFS_ORDER_HEADER">
                    <xsd:documentation>The date which was used to determine the conversion rate.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReportingConversionRate" type="yfctype:ConversionRate" use="optional">
                <xsd:annotation yfc:columnName="REPORTING_CONVERSION_RATE" yfc:tableName="YFS_ORDER_HEADER">
                    <xsd:documentation>
                        <div>
			The conversion rate from the order's original currency to the Enterprise's currency. This is not used internally within the system. This field is provided so that reports can be built in a standard currency for an Enterprise.
			<br/>
			If default reporting converstion Rate flag is checked in currency definition setup, ReportingConversionRate value passed in i/p xml will be ignored and the value would be based on currency conversion setup.
			</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="HeaderCharges">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:HeaderCharge"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="HeaderCharge">
        <xsd:annotation yfc:tableName="YFS_HEADER_CHARGES">
            <xsd:documentation>This table stores charges that are applied at the header level.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ChargeAmount" type="yfctype:PlusMinusMoney" use="optional">
                <xsd:annotation yfc:columnName="CHARGE" yfc:tableName="YFS_HEADER_CHARGES">
                    <xsd:documentation>The amount of the charge.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ChargeCategory" type="yfctype:ChargeCategory" use="required">
                <xsd:annotation yfc:columnName="CHARGE_CATEGORY" yfc:tableName="YFS_HEADER_CHARGES">
                    <xsd:documentation>
                        <div>
			The charge category. For example, Labeling or Handling.
			<br/>The validation is done only when the <code>ValidateChargeNameandCategory</code> rule is set to <b>
                                <code>Yes</code>
                            </b>.
			The same combination of <code>ChargeCategory</code> and <code>ChargeName</code> must not be passed multiple times for a given order.
   			<Validation>
                                <Condition>The same combination of <code>ChargeCategory</code> and <code>ChargeName</code> is passed multiple item for the same order.
				</Condition>
                                <Error Description="Duplicate Charge." ErrorCode="YFS10332"/>
                            </Validation>
                            <Validation>
                                <Condition>The <code>ChargeCategory</code> is not passed.
				</Condition>
                                <Error Description="Blank charge category." ErrorCode="YFS10343"/>
                            </Validation>
                            <Validation>
                                <Condition>Either of the <code>ChargeCategory</code>  and <code>ChargeName</code> are not a valid combination for the document, or for the ruleset of the Enterprise. The table <code>YFS_CHARGE_NAME</code> is used as part of this check.
				</Condition>
                                <Error Description="Invalid charge." ErrorCode="YFS10346"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ChargeName" type="yfctype:ChargeName" use="optional">
                <xsd:annotation yfc:columnName="CHARGE_NAME" yfc:tableName="YFS_HEADER_CHARGES">
                    <xsd:documentation>The name of the charge. For example, Price Ticket.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsManual" type="yfctype:EnumIsManualCharge" use="optional">
                <xsd:annotation yfc:columnName="IS_MANUAL" yfc:tableName="YFS_HEADER_CHARGES">
                    <xsd:documentation>Determines whether the charge was manually created. Valid Values: Y, N, or M. Default Value: Y</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Reference" type="yfctype:Text-50" use="optional">
                <xsd:annotation yfc:columnName="REFERENCE" yfc:tableName="YFS_HEADER_CHARGES">
                    <xsd:documentation>Reference information about this charge.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="HeaderTaxes">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:HeaderTax"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="HeaderTax">
        <xsd:annotation yfc:tableName="YFS_TAX_BREAKUP">
            <xsd:documentation>
                <div>
						Contains tax related information for various entities in the system.  If <code>ChargeCategory</code> and <code>ChargeName</code> are not provided, tax will be considered as a billable and positive charge.  If not implementing <code>YFSRecalculateLineTaxUE</code>, <code>ChargeCategory</code>, <code>Tax</code>, and <code>TaxPercentage</code> are required for the default repricing logic.
					</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ChargeCategory" type="yfctype:ChargeCategory" use="optional">
                <xsd:annotation yfc:columnName="CHARGE_CATEGORY" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>The charge category for which tax is being charged. For example, Labeling or Handling.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ChargeName" type="yfctype:ChargeName" use="optional">
                <xsd:annotation yfc:columnName="CHARGE_NAME" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Charge name for which tax is being charged. For example, Price Ticket.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Reference1" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="REFERENCE1" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Additional reference information. This field can be customized by the Enterprise.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Reference2" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="REFERENCE2" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Additional reference information. This field can be customized by the Enterprise.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Reference3" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="REFERENCE3" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Additional reference information. This field can be customized by the Enterprise.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Tax" type="yfctype:PlusMinusMoney" use="optional">
                <xsd:annotation yfc:columnName="TAX" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>
                        <div>
						The tax amount. <code>Tax</code> cannot be less than zero.  During the default repricing logic, this is recalculated using the <code>TaxPercentage</code> and the price of the related <code>ChargeCategory</code>.  This can be overridden with the <code>YFSRecalulateHeaderTaxUE</code>.
						<Validation>
                                <Condition>Negative amount is passed.
							</Condition>
                                <Error Description="Negative Tax." ErrorCode="YFS10420"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxName" type="yfctype:Text-40" use="required">
                <xsd:annotation yfc:columnName="TAX_NAME" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>
                        <div>
					Type of tax. The <code>TaxName</code> must only be passed once, even when using a combination of <code>ChargeCategory</code> and <code>ChargeName</code>.
					<Validation>
                                <Condition>Must be provided if <code>HeaderTaxes</code> is passed.
						</Condition>
                                <Error Description="Blank Tax Name." ErrorCode="YFS10344"/>
                            </Validation>
                            <Validation>
                                <Condition>If a combination of ChargeCategory and ChargeName is used, and  <code>TaxName</code> is passed multiple times.
						</Condition>
                                <Error Description="Duplicate Tax." ErrorCode="YFS10334"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxPercentage" type="yfctype:TaxType" use="optional">
                <xsd:annotation yfc:columnName="TAX_PERCENTAGE" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Percentage amount for tax calculation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxableFlag" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="TAXABLE_FLAG" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>This flag indicates if a charge can be taxed.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PaymentMethods">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:PaymentMethod"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PaymentMethod">
        <xsd:annotation yfc:tableName="YFS_PAYMENT">
            <xsd:documentation>This table stores details about the customer's payment methods. The key to this table is posted in YFS_CHARGE_TRANSACTION table.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" name="PaymentDetails">
                    <xsd:annotation yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                        <xsd:documentation>This table stores detailed information about authorizations and charges made against all supported payment types. Typically, this information would be returned by credit card interface programs such as CyberSource or PaymentTek.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:attribute name="AuditTransactionId" type="yfctype:TransactionID" use="optional">
                            <xsd:annotation yfc:columnName="AUDIT_TRANSACTION_ID" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>
                                    <div>
									AuditTransactionID is a user defined data. This data will be stored on YFS_ORDER_AUDIT table. Also in case of APIs that cause the creation of a CHARGE_REQUEST, the same field will be stored on YFS_CHARGE_TRANSACTION table.
								</div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AuthAvs" type="yfctype:Text-20" use="optional">
                            <xsd:annotation yfc:columnName="AUTH_AVS" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The AVS authorization code.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AuthCode" type="yfctype:Text-20" use="optional">
                            <xsd:annotation yfc:columnName="AUTH_CODE" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The authorization code.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AuthReturnCode" type="yfctype:Text-20" use="optional">
                            <xsd:annotation yfc:columnName="AUTH_RETURN_CODE" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The authorization return code.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AuthReturnFlag" type="yfctype:Text-20" use="optional">
                            <xsd:annotation yfc:columnName="AUTH_RETURN_FLAG" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The authorization return flag.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AuthReturnMessage" type="yfctype:Text-255" use="optional">
                            <xsd:annotation yfc:columnName="AUTH_RETURN_MESSAGE" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The authorization return message.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AuthTime" type="yfctype:TimeStamp" use="optional">
                            <xsd:annotation yfc:columnName="AUTH_TIME" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The authorization time.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AuthorizationExpirationDate" type="yfctype:TimeStamp" use="optional">
                            <xsd:annotation yfc:columnName="AUTHORIZATION_EXPIRATION_DATE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The date and time until which the authorization specified by the authorization ID is applicable.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AuthorizationID" type="yfctype:Text-255" use="optional">
                            <xsd:annotation yfc:columnName="AUTHORIZATION_ID" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>For authorization requests, this field holds the identifier of the authorization that was made. For charge requests, this field holds the authroization ID to be used for charging the amount.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CVVAuthCode" type="yfctype:Text-20" use="optional">
                            <xsd:annotation yfc:columnName="CVV_AUTH_CODE" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The CVV authorization code.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ChargeType" type="yfctype:ChargeCategory" use="required">
                            <xsd:annotation yfc:columnName="CHARGE_TYPE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>
                                    <div>
						The type of charge that is being applied to the order.
		 				<br/>Valid Values: <b>
                                    <code>AUTHORIZATION</code>
                                    </b> or <b>
                                    <code>CHARGE</code>
                                    </b>
                                    <Validation>
                                    <Condition> Invalid value passed.
							</Condition>
                                    <Error Description="YFS:Invalid Transaction Type." ErrorCode="YFS10156"/>
                                    </Validation>
                                    </div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CollectionDate" type="yfctype:TimeStamp" use="optional">
                            <xsd:annotation yfc:columnName="COLLECTION_DATE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>
                                    <div>
									The date and time when the charge is realized.It can be future date.When this date is passed in input xml, <code>Is_Collection_Date_Firm </code>flag is set to Y.
								</div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="HoldAgainstBook" type="yfctype:Flag" use="optional">
                            <xsd:annotation yfc:columnName="HOLD_AGAINST_BOOK" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>
                                    <div>
									This flag indicates whether or not pre-authorized or pre-charged amounts for the entire order will be held until the order is completely invoiced. If passed as 'Y', they will be. If passed as 'N', or not passed, the extra charge or authorization amount is not held. For example, if there are two units of $5 each on an pre-paid order, and $10 are collected up front, then when <code>HoldAgainstBook</code> is passed as 'N', a refund for $5 is created upon invoicing of the first unit. When passed as 'Y', no refund is created. If the pre-charged amount is $12 for the order, and when <code>HoldRefundAgainstBook</code> is passed as 'N', a refund is created for $7 upon invoicing of the first unit, and for $2 when <code>HoldAgainstBook</code> is passed as 'Y'.

								</div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="InternalReturnCode" type="yfctype:Text-20" use="optional">
                            <xsd:annotation yfc:columnName="INTERNAL_RETURN_CODE" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The internal return code.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="InternalReturnFlag" type="yfctype:Text-20" use="optional">
                            <xsd:annotation yfc:columnName="INTERNAL_RETURN_FLAG" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The internal return flag.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="InternalReturnMessage" type="yfctype:Text-255" use="optional">
                            <xsd:annotation yfc:columnName="INTERNAL_RETURN_MESSAGE" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The internal return message.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ProcessedAmount" type="yfctype:Money" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>
							If  <code>ProcessedAmount</code> is passed as a value greater than zero, the <code>STATUS</code> in the  <code>CHARGE_TRANSACTION</code> table is set to <b>
                                    <code>CLOSED</code>
                                    </b>.
							<br/>If <code>ProcessedAmount</code> is passed as zero or not passed any attributes of this element associated with the <code>YFS_CHARGE_TRANSACTION</code> table will be lost.
							<br/>The <code>ProcessedAmount</code> is used to update  <code>CHARGE_TRANSACTION</code> table, depending on the charge type.  If the <code>ChargeType</code> value is
							<br/>
                                    <b>
                                    <code>AUTHORIZATION</code>
                                    </b> - the <code>OpenAuthorizedAmount</code> is updated with the <code>ProcessedAmount</code>.
							<br/>
                                    <b>
                                    <code>CHARGE</code>
                                    </b> - the <code>CreditAmount</code> is updated with the <code>ProcessedAmount</code>.
							</div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Reference1" type="yfctype:Text-100" use="optional">
                            <xsd:annotation yfc:columnName="REFERENCE1" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The reference 1.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Reference2" type="yfctype:Text-100" use="optional">
                            <xsd:annotation yfc:columnName="REFERENCE2" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The reference 2.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="RequestAmount" type="yfctype:Money" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>
							If <code>RequestAmount</code> is passed, the value is used to update the <code>RequestAmount</code> of the  <code>CHARGE_TRANSACTION</code> table, and the <code>TRAN_AMOUNT</code> of the <code>CREDIT_CARD_TRANSACTION</code> table.
							</div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="RequestId" type="yfctype:Text-255" use="optional">
                            <xsd:annotation yfc:columnName="REQUEST_ID" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The request identifier returned by the transaction.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="RequestProcessed" type="yfctype:Flag" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>
									If the value of this is <b>
                                    <code>Y</code>
                                    </b>, the payment details record will be treated as already processed. This flag is considered only when <code>ProcessedAmount</code> is <b>
                                    <code>0</code>
                                    </b>. Request with <code>ProcessedAmount</code> greater than 	<b>
                                    <code>0</code>
                                    </b> is always considered as processed.
								</div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TranRequestTime" type="yfctype:TimeStamp" use="optional">
                            <xsd:annotation yfc:columnName="TRAN_REQUEST_TIME" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The time at which the transaction request was made.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TranReturnCode" type="yfctype:Text-20" use="optional">
                            <xsd:annotation yfc:columnName="TRAN_RETURN_CODE" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The return code for the transaction.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TranReturnFlag" type="yfctype:Text-20" use="optional">
                            <xsd:annotation yfc:columnName="TRAN_RETURN_FLAG" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The flag returned by the transaction.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TranReturnMessage" type="yfctype:Text-255" use="optional">
                            <xsd:annotation yfc:columnName="TRAN_RETURN_MESSAGE" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The message returned by the transaction.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TranType" type="yfctype:Text-40" use="optional">
                            <xsd:annotation yfc:columnName="TRAN_TYPE" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The transaction type.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:PaymentDetailsList"/>
                <xsd:element maxOccurs="1" minOccurs="0" name="PersonInfoBillTo">
                    <xsd:annotation yfc:tableName="YFS_PERSON_INFO">
                        <xsd:documentation>This table stores relevant personal information and addresses. Addresses are typically organization address, ship node address, customer address, and so forth. Personal information is required in cases where person to person contact needs to be established; for example, taking appointments or speaking during negotiations. The key of this table is posted in various tables to allow access to people and address information.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:attribute name="AddressID" type="yfctype:Text-256" use="optional">
                            <xsd:annotation yfc:columnName="ADDRESS_ID" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>A user specified label for an address.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AddressLine1" type="yfctype:Address" use="optional">
                            <xsd:annotation yfc:columnName="ADDRESS_LINE1" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The address line 1 field.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AddressLine2" type="yfctype:Address" use="optional">
                            <xsd:annotation yfc:columnName="ADDRESS_LINE2" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The address line 2 field.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AddressLine3" type="yfctype:Address" use="optional">
                            <xsd:annotation yfc:columnName="ADDRESS_LINE3" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The address line 3 field.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AddressLine4" type="yfctype:Address" use="optional">
                            <xsd:annotation yfc:columnName="ADDRESS_LINE4" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The address line 4 field.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AddressLine5" type="yfctype:Address" use="optional">
                            <xsd:annotation yfc:columnName="ADDRESS_LINE5" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The address line 5 field.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AddressLine6" type="yfctype:Address" use="optional">
                            <xsd:annotation yfc:columnName="ADDRESS_LINE6" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The address line 6 field.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AlternateEmailID" type="yfctype:EMailID" use="optional">
                            <xsd:annotation yfc:columnName="ALTERNATE_EMAILID" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The alternate email address at which the person or a predesignated employee of the organization referred to in the address can be contacted.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Beeper" type="yfctype:PhoneNo" use="optional">
                            <xsd:annotation yfc:columnName="BEEPER" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The beeper number at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="City" type="yfctype:City" use="optional">
                            <xsd:annotation yfc:columnName="CITY" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The city in the address.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Company" type="yfctype:Company" use="optional">
                            <xsd:annotation yfc:columnName="COMPANY" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The company this person is associated with.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Country" type="yfctype:Country" use="optional">
                            <xsd:annotation yfc:columnName="COUNTRY" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The country in the address.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="DayFaxNo" type="yfctype:PhoneNo" use="optional">
                            <xsd:annotation yfc:columnName="DAY_FAX_NO" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The day time fax at which the person or a predesignated employee of the organization referred to in the address can be notified.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="DayPhone" type="yfctype:PhoneNo" use="optional">
                            <xsd:annotation yfc:columnName="DAY_PHONE" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The day time phone at which the person or organization referred to in the address can be called.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Department" type="yfctype:Department" use="optional">
                            <xsd:annotation yfc:columnName="DEPARTMENT" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The primary department in charge of procurement.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="EMailID" type="yfctype:EMailID" use="optional">
                            <xsd:annotation yfc:columnName="EMAILID" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The email address at which the person or a predesignated employee of the organization referred to in the address can be contacted.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="EveningFaxNo" type="yfctype:PhoneNo" use="optional">
                            <xsd:annotation yfc:columnName="EVENING_FAX_NO" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The evening time fax at which the person or a predesignated employee of the organization referred to in the address can be notified.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="EveningPhone" type="yfctype:PhoneNo" use="optional">
                            <xsd:annotation yfc:columnName="EVENING_PHONE" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The evening time phone at which the person or organization referred to in the address can be called.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="FirstName" type="yfctype:PersonName" use="optional">
                            <xsd:annotation yfc:columnName="FIRST_NAME" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The first name of the person.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="IsAddressVerified" type="yfctype:Flag" use="optional">
                            <xsd:annotation yfc:columnName="IS_ADDRESS_VERIFIED" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>This flag indicates whether or not the address in this record has been verified by an address verification program. Intended for future use.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="IsCommercialAddress" type="yfctype:Flag" use="optional">
                            <xsd:annotation yfc:columnName="IS_COMMERCIAL_ADDRESS" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>Indicates whether the address is commercial, residential or none. Values of this field can be Y -Commercial address.N - Residential address. undefined for Null.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="JobTitle" type="yfctype:Title-40" use="optional">
                            <xsd:annotation yfc:columnName="JOB_TITLE" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The job title of the person.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="LastName" type="yfctype:PersonName" use="optional">
                            <xsd:annotation yfc:columnName="LAST_NAME" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The last name of the person.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Latitude" use="optional">
                            <xsd:annotation yfc:columnName="LATITUDE" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The latitude of this location.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:DecimalWithNegative"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                </xsd:union>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="Longitude" use="optional">
                            <xsd:annotation yfc:columnName="LONGITUDE" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The longitude of this location.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:DecimalWithNegative"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                </xsd:union>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="MiddleName" type="yfctype:MiddleName" use="optional">
                            <xsd:annotation yfc:columnName="MIDDLE_NAME" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The middle name of the person.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="MobilePhone" type="yfctype:PhoneNo" use="optional">
                            <xsd:annotation yfc:columnName="MOBILE_PHONE" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The mobile phone at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OtherPhone" type="yfctype:PhoneNo" use="optional">
                            <xsd:annotation yfc:columnName="OTHER_PHONE" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The alternate phone number at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PersonID" type="yfctype:PersonId" use="optional">
                            <xsd:annotation yfc:columnName="PERSON_ID" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The unique identifier for a person.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PersonInfoKey" type="yfctype:Key" use="optional">
                            <xsd:annotation yfc:columnName="PERSON_INFO_KEY" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The primary key for the YFS_PERSON_INFO table.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="State" type="yfctype:State" use="optional">
                            <xsd:annotation yfc:columnName="STATE" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The state in the address.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Suffix" type="yfctype:Suffix" use="optional">
                            <xsd:annotation yfc:columnName="SUFFIX" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The suffix for a person, such as Jr., or Sr.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TaxGeoCode" type="yfctype:GeoCode" use="optional">
                            <xsd:annotation yfc:columnName="TAX_GEO_CODE" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The tax geo code for this location. Intended for future use.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Title" type="yfctype:Title-10" use="optional">
                            <xsd:annotation yfc:columnName="TITLE" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The title for the person such as Ms., Mr., or Dr..</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ZipCode" type="yfctype:ZipCode" use="optional">
                            <xsd:annotation yfc:columnName="ZIP_CODE" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The zip code in the address.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
            </xsd:all>
            <xsd:attribute name="BillToKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="BILL_TO_KEY" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The bill to address that is associated with this payment. Can be passed if different from the bill to address on the order.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ChargeSequence" type="yfctype:ShorterNumber" use="optional">
                <xsd:annotation yfc:columnName="CHARGE_SEQUENCE" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>
                        <div>
						Used to identify the order to which charges are applied.
						<br/>
						 The Seller Organization can configure the payment types so that they are charged and refunded in a particular sequence. The charge sequence and refund sequence are independent of each other. A <code>PaymentType</code> with a <code>ChargeSequence</code> set to <b>
                                <code>0</code>
                            </b> is charged before another <code>PaymentType</code> with <code>ChargeSequence</code> set to <b>
                                <code>1</code>
                            </b>.
						 <br/> The Buyer can also specify the <code>ChargeSequence</code>  of the <code>PaymentMethod</code> in the <code>createOrder</code> input xml. If a charge sequence is not specified by the Buyer, the Seller's <code>ChargeSequence</code> is followed.
						 <br/>
                            <br/>If both the Buyer and the Seller supply <code>ChargeSequence</code>, charges are made in the order
						 that the Seller specifies, but within a particular charge type, such as CreditCard, the Buyer's sequence is honored.
						<br/>
                            <br/>For example:
						<br/>A seller specifies the  <code>ChargeSequence</code> as 0 for CUSTOMER_ACCOUNT, 1 for CREDIT_CARD, 2 for OTHER.
						<br/>A buyer specifies the <code>ChargeSequence</code> as 1 for MyBank-CreditCard, 2 for CUSTOMER_ACCOUNT, and 3 for  MyOther-CreditCard.
						<br/>The payment methods are charged in the following sequence:
					    <br/>CUSTOMER_ACCOUNT, MyBank-CreditCard,  MyOther-CreditCard.
						<br/>The Customer account is charged because that is the seller's preferred method, and the two credit cards are charged based on the order that the buyer specified.
					</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CheckNo" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="CHEQUE_NO" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The cheque number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CheckReference" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="CHEQUE_REFERENCE" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The cheque reference.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CreditCardExpDate" type="yfctype:CreditCardExpDate" use="optional">
                <xsd:annotation yfc:columnName="CREDIT_CARD_EXP_DATE" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The expiration date for the credit card.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CreditCardName" type="yfctype:Text-300" use="optional">
                <xsd:annotation yfc:columnName="CREDIT_CARD_NAME" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The credit card name.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CreditCardNo" type="yfctype:Text-1000" use="optional">
                <xsd:annotation yfc:columnName="CREDIT_CARD_NO" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>
                        <div>
				Credit Card number. Values must be passed for  <code>CreditCardNo</code>,  <code>CustomerAccountNo</code> and <code>PaymentReference1</code> for the <code>PaymentMethod</code> to be complete. If all of these attributes are not  passed the <code>INCOMPLETE_PAYMENT_TYPE</code> is set to <b>
                                <code>Y</code>
                            </b> for the <code>PaymentMethod</code>.
				</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CreditCardType" type="yfctype:Text-32" use="optional">
                <xsd:annotation yfc:columnName="CREDIT_CARD_TYPE" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The credit card type group.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CustomerAccountNo" type="yfctype:AccountNo" use="optional">
                <xsd:annotation yfc:columnName="CUSTOMER_ACCOUNT_NO" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>
                        <div>
				Number that identifies the customer account. Values must be passed for  <code>CreditCardNo</code>,  <code>CustomerAccountNo</code> and <code>PaymentReference1</code> for the <code>PaymentMethod</code> to be complete. If they are not all passed the <code>INCOMPLETE_PAYMENT_TYPE</code> is set to <b>
                                <code>Y</code>
                            </b> for the <code>PaymentMethod</code>.
				</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CustomerPONo" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="CUSTOMER_PO_NO" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The customer PO account number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DisplayCreditCardNo" type="yfctype:Text-4" use="optional">
                <xsd:annotation yfc:columnName="DISPLAY_CREDIT_CARD_NO" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The four digits of the credit card number that should be displayed.  If not passed, it is defaulted to be the last four digits of the credit card number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DisplayCustomerAccountNo" type="yfctype:Text-4" use="optional">
                <xsd:annotation yfc:columnName="DISPLAY_CUST_ACCT_NO" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The four digits of the customer account number that should be displayed.  If not passed, it is defaulted to be the last four digits of the customer account number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DisplayPaymentReference1" type="yfctype:Text-4" use="optional">
                <xsd:annotation yfc:columnName="DISPLAY_PAYMENT_REF1" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The four digits of the payment reference 1 that should be displayed.  If not passed, it is defaulted to be the last four digits of the payment reference 1.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DisplaySvcNo" type="yfctype:Text-4" use="optional">
                <xsd:annotation yfc:columnName="DISPLAY_SVC_NO" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The four digits of the stored value card that should be displayed.  If not passed, it is defaulted to be the last four digits of the stored value card number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FirstName" type="yfctype:Text-300" use="optional">
                <xsd:annotation yfc:columnName="FIRST_NAME" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The first name of the credit card holder.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LastName" type="yfctype:Text-300" use="optional">
                <xsd:annotation yfc:columnName="LAST_NAME" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The last name of the credit card holder.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MaxChargeLimit" type="yfctype:Money" use="optional">
                <xsd:annotation yfc:columnName="MAX_CHARGE_LIMIT" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The maximum amount that can be charged on this payment method. However, if UnlimitedCharges=Y, this amount will not be considered.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MiddleName" type="yfctype:Text-300" use="optional">
                <xsd:annotation yfc:columnName="MIDDLE_NAME" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The middle name of the credit card holder.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PaymentReference1" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="PAYMENT_REFERENCE1" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>
                        <div>
					 Values must be passed for  <code>CreditCardNo</code>,  <code>CustomerAccountNo</code> and <code>PaymentReference1</code> for the <code>PaymentMethod</code> to be complete. If they are not all passed the <code>INCOMPLETE_PAYMENT_TYPE</code> is set to <b>
                                <code>Y</code>
                            </b> for the <code>PaymentMethod</code>.
				 </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PaymentReference2" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="PAYMENT_REFERENCE2" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The payment reference 2.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PaymentReference3" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="PAYMENT_REFERENCE3" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The payment reference 3.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PaymentReference4" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="PAYMENT_REFERENCE4" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The payment reference 4. This does not identify the payment method uniquely.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PaymentReference5" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="PAYMENT_REFERENCE5" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The payment reference 5. This does not identify the payment method uniquely.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PaymentReference6" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="PAYMENT_REFERENCE6" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The payment reference 6. This does not identify the payment method uniquely.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PaymentReference7" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="PAYMENT_REFERENCE7" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The payment reference 7. This does not identify the payment method uniquely.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PaymentReference8" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="PAYMENT_REFERENCE8" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The payment reference 8. This does not identify the payment method uniquely.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PaymentReference9" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="PAYMENT_REFERENCE9" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The payment reference 9. This does not identify the payment method uniquely.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PaymentType" type="yfctype:Text-20" use="required">
                <xsd:annotation yfc:columnName="PAYMENT_TYPE" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>
                        <div>
				Type of payment.
				<br/>The <code>PaymentType</code> must be a value that is already configured for the Seller Organization.  <br/>If a   <code>PaymentMethod</code> element is passed, the <code>PaymentType</code> is validated, even if payment processing is not required for the order.
				<Validation>
                                <Condition>Must be a value defined for the Seller Organization.
					</Condition>
                                <Error Description="Payment Type not set up for seller." ErrorCode="YFS10491"/>
                            </Validation>
                            <Validation>
                                <Condition>
                                    <code>PaymentType</code> must be passed.
					</Condition>
                                <Error Description="Mandatory Parameters for the Operation are missing." ErrorCode="YFS10364"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReducePendingAsyncronousAmounts" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>Indicates if external charges are being recorded for an asynchronous request. Valid values are 'Y', 'N' or Void. Set this flag to "Y" with a valid <code>RequestAmount</code> to indicate that the external charges are being recorded for an asynchronous request. This will enable the system to adjust the pending asynchronous amounts correctly. Set the flag to "N" to indicate that the external charges are being recorded for non-asynchronous request. In this mode, system will not process any pending asynchronous amounts. Set this flag to Void or Do not pass this flag, if unsure the request is asynchronous or not. In this mode system will try to adjust any pending asynchronous amounts that exists for the request. This mode is provided for backward compatibility.
						   </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SvcNo" type="yfctype:Text-1000" use="optional">
                <xsd:annotation yfc:columnName="SVC_NO" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The stored value card number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnlimitedCharges" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="UNLIMITED_CHARGES" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>Indicates if there is a limit to the amount that can be charged on the payment method. If not passed, defaults to Y.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PaymentDetailsList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" name="PaymentDetails">
                    <xsd:annotation yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                        <xsd:documentation>
                            <div>
                                <br/> This element cannot be passed for orders with <code>pending changes</code>.
						 </div>
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:attribute name="AuditTransactionId" type="yfctype:TransactionID" use="optional">
                            <xsd:annotation yfc:columnName="AUDIT_TRANSACTION_ID" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>
                                    <div>
										AuditTransactionID is a user defined data. This data will be stored on YFS_ORDER_AUDIT table. Also in case of APIs that cause the creation of a CHARGE_REQUEST, the same field will be stored on YFS_CHARGE_TRANSACTION table.
									</div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AuthAvs" type="yfctype:Text-20" use="optional">
                            <xsd:annotation yfc:columnName="AUTH_AVS" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The AVS authorization code.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AuthCode" type="yfctype:Text-20" use="optional">
                            <xsd:annotation yfc:columnName="AUTH_CODE" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The authorization code.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AuthReturnCode" type="yfctype:Text-20" use="optional">
                            <xsd:annotation yfc:columnName="AUTH_RETURN_CODE" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The authorization return code.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AuthReturnFlag" type="yfctype:Text-20" use="optional">
                            <xsd:annotation yfc:columnName="AUTH_RETURN_FLAG" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The authorization return flag.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AuthReturnMessage" type="yfctype:Text-255" use="optional">
                            <xsd:annotation yfc:columnName="AUTH_RETURN_MESSAGE" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The authorization return message.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AuthTime" type="yfctype:TimeStamp" use="optional">
                            <xsd:annotation yfc:columnName="AUTH_TIME" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The authorization time.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AuthorizationExpirationDate" type="yfctype:TimeStamp" use="optional">
                            <xsd:annotation yfc:columnName="AUTHORIZATION_EXPIRATION_DATE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The date and time until which the authorization specified by the authorization ID is applicable.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AuthorizationID" type="yfctype:Text-255" use="optional">
                            <xsd:annotation yfc:columnName="AUTHORIZATION_ID" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>For authorization requests, this field holds the identifier of the authorization that was made. For charge requests, this field holds the authroization ID to be used for charging the amount.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CVVAuthCode" type="yfctype:Text-20" use="optional">
                            <xsd:annotation yfc:columnName="CVV_AUTH_CODE" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The CVV authorization code.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ChargeType" type="yfctype:ChargeCategory" use="required">
                            <xsd:annotation yfc:columnName="CHARGE_TYPE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>
                                    <div>
							The type of charge that is being applied to the order.
							<br/>Valid Values: <b>
                                    <code>AUTHORIZATION</code>
                                    </b> or <b>
                                    <code>CHARGE</code>
                                    </b>
                                    <Validation>
                                    <Condition> Invalid value passed.
								</Condition>
                                    <Error Description="YFS:Invalid Transaction Type." ErrorCode="YFS10156"/>
                                    </Validation>
                                    </div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CollectionDate" type="yfctype:TimeStamp" use="optional">
                            <xsd:annotation yfc:columnName="COLLECTION_DATE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>
                                    <div>
									The date and time when the charge is realized.It can be future date.When this date is passed in input xml, <code>Is_Collection_Date_Firm </code>flag is set to Y.
									</div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="HoldAgainstBook" type="yfctype:Flag" use="optional">
                            <xsd:annotation yfc:columnName="HOLD_AGAINST_BOOK" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>
                                    <div>
										This flag indicates whether or not pre-authorized or pre-charged amounts for the entire order will be held until the order is completely invoiced. If passed as 'Y', they will be. If passed as 'N', or not passed, the extra charge or authorization amount is not held. For example, if there are two units of $5 each on an pre-paid order, and $10 are collected up front, then when <code>HoldAgainstBook</code> is passed as 'N', a refund for $5 is created upon invoicing of the first unit. When passed as 'Y', no refund is created. If the pre-charged amount is $12 for the order, and when <code>HoldRefundAgainstBook</code> is passed as 'N', a refund is created for $7 upon invoicing of the first unit, and for $2 when <code>HoldAgainstBook</code> is passed as 'Y'.

									</div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="InternalReturnCode" type="yfctype:Text-20" use="optional">
                            <xsd:annotation yfc:columnName="INTERNAL_RETURN_CODE" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The internal return code.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="InternalReturnFlag" type="yfctype:Text-20" use="optional">
                            <xsd:annotation yfc:columnName="INTERNAL_RETURN_FLAG" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The internal return flag.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="InternalReturnMessage" type="yfctype:Text-255" use="optional">
                            <xsd:annotation yfc:columnName="INTERNAL_RETURN_MESSAGE" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The internal return message.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ProcessedAmount" type="yfctype:Money" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>
								If  <code>ProcessedAmount</code> is passed as a value greater than zero, the <code>STATUS</code> in the  <code>CHARGE_TRANSACTION</code> table is set to <b>
                                    <code>CLOSED</code>
                                    </b>.
								<br/>If <code>ProcessedAmount</code> is passed as zero or not passed any attributes of this element associated with the <code>YFS_CHARGE_TRANSACTION</code> table will be lost.
								<br/>The <code>ProcessedAmount</code> is used to update  <code>CHARGE_TRANSACTION</code> table, depending on the charge type.  If the <code>ChargeType</code> value is
								<br/>
                                    <b>
                                    <code>AUTHORIZATION</code>
                                    </b> - the <code>OpenAuthorizedAmount</code> is updated with the <code>ProcessedAmount</code>.
								<br/>
                                    <b>
                                    <code>CHARGE</code>
                                    </b> - the <code>CreditAmount</code> is updated with the <code>ProcessedAmount</code>.
								</div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Reference1" type="yfctype:Text-100" use="optional">
                            <xsd:annotation yfc:columnName="REFERENCE1" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The reference 1.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Reference2" type="yfctype:Text-100" use="optional">
                            <xsd:annotation yfc:columnName="REFERENCE2" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The reference 2.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="RequestAmount" type="yfctype:Money" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>
								If <code>RequestAmount</code> is passed, the value is used to update the <code>RequestAmount</code> of the  <code>CHARGE_TRANSACTION</code> table, and the <code>TRAN_AMOUNT</code> of the <code>CREDIT_CARD_TRANSACTION</code> table.
								</div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="RequestId" type="yfctype:Text-255" use="optional">
                            <xsd:annotation yfc:columnName="REQUEST_ID" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The request identifier returned by the transaction.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="RequestProcessed" type="yfctype:Flag" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>
										If the value of this is <b>
                                    <code>Y</code>
                                    </b>, the payment details record will be treated as already processed. This flag is considered only when <code>ProcessedAmount</code> is <b>
                                    <code>0</code>
                                    </b>. Request with <code>ProcessedAmount</code> greater than 	<b>
                                    <code>0</code>
                                    </b> is always considered as processed.
									</div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TranRequestTime" type="yfctype:TimeStamp" use="optional">
                            <xsd:annotation yfc:columnName="TRAN_REQUEST_TIME" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The time at which the transaction request was made.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TranReturnCode" type="yfctype:Text-20" use="optional">
                            <xsd:annotation yfc:columnName="TRAN_RETURN_CODE" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The return code for the transaction.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TranReturnFlag" type="yfctype:Text-20" use="optional">
                            <xsd:annotation yfc:columnName="TRAN_RETURN_FLAG" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The flag returned by the transaction.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TranReturnMessage" type="yfctype:Text-255" use="optional">
                            <xsd:annotation yfc:columnName="TRAN_RETURN_MESSAGE" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The message returned by the transaction.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TranType" type="yfctype:Text-40" use="optional">
                            <xsd:annotation yfc:columnName="TRAN_TYPE" yfc:tableName="YFS_CREDIT_CARD_TRANSACTION">
                                <xsd:documentation>The transaction type.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="SpecialServices">
        <xsd:annotation yfc:tableName="YFS_SPECIAL_SERVICES">
            <xsd:documentation>This table stores special services supported by various carriers that can be used when shipping packages. Examples of special services are signature required upon delivery of the package, Saturday pick-up and Saturday delivery.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:SpecialService"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="SpecialService">
        <xsd:annotation yfc:tableName="YCS_SPECIAL_SERVICE">
            <xsd:documentation>This table stores the special services supported by Carriers.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="SpecialServicesCode" type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="SPECIAL_SERVICES_CODE" yfc:tableName="YFS_SPECIAL_SERVICE_REF">
                    <xsd:documentation>
                        <div>
			The code identifying the Special Service to be performed on the Order.
			<Validation>
                                <Condition>
                                    <code>SpecialServicesCode</code> must be defined in the configuration
				</Condition>
                                <Error Description="Special service code does not exist" ErrorCode="OMP11027"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="AnswerSets">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:AnswerSet"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="AnswerSet">
        <xsd:annotation yfc:tableName="YFS_ANSWER_SET_TRAN">
            <xsd:documentation>Stores the relationship between a set of answers and the entities which provided these answers.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:Answers"/>
            </xsd:all>
            <xsd:attribute name="PersonInfoKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="PERSON_INFO_KEY" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The person info record to store this answer set for.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="QuestionType" type="yfctype:Text-20" use="required">
                <xsd:annotation yfc:columnName="QUESTION_TYPE" yfc:tableName="YFS_QUESTION">
                    <xsd:documentation>
                        <div>The question type associated to the defined answer set. 
						<Validation>
                                <Condition>
                                    <code>QuestionType</code> not passed.
								</Condition>
                                <Error Description="Mandatory attributes for answer set are missing." ErrorCode="YCP73_00005"/>
                            </Validation>
                            <Validation>
                                <Condition>Invalid <code>QuestionType</code>.
								</Condition>
                                <Error Description="Invalid Question Attribute." ErrorCode="YCP73_00006"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Answers">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:Answer"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Answer">
        <xsd:annotation yfc:tableName="YFS_ANSWER_TRAN">
            <xsd:documentation>Stores each answer for an answer set.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="AnswerValue" type="yfctype:Text-1000" use="optional">
                <xsd:annotation yfc:columnName="ANSWER_VALUE" yfc:tableName="YFS_ANSWER_TRAN">
                    <xsd:documentation>
                        <div>The answer for the question.
								<Validation>
                                <Condition>
                                    <code>AnswerValue</code> passed should be a valid answer for the question it belongs to.</Condition>
                                <Error Description="Answer is not one of the valid pre-defined answer options." ErrorCode="YCP73_00007"/>
                                <Error Description="Answer for this question should be an integer number." ErrorCode="YCP73_00020"/>
                                <Error Description="Answer for this question should be a decimal number." ErrorCode="YCP73_00021"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="QuestionID" type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="QUESTION_ID" yfc:tableName="YFS_ANSWER_TRAN">
                    <xsd:documentation>
                        <div>Identifier for the question that this answer is for.
								<Validation>
                                <Condition>
                                    <code>QuestionID</code> should be valid.</Condition>
                                <Error Description="Invalid Question Attribute." ErrorCode="YCP73_00006"/>
                                <Error Description="Question should only be answered when parent answer option is selected." ErrorCode="YCP73_00009"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PersonInfoSoldTo">
        <xsd:annotation yfc:tableName="YFS_PERSON_INFO">
            <xsd:documentation>This table stores relevant personal information and addresses. Addresses are typically organization address, ship node address, customer address, and so forth. Personal information is required in cases where person to person contact needs to be established; for example, taking appointments or speaking during negotiations. The key of this table is posted in various tables to allow access to people and address information.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="AddressID" type="yfctype:Text-256" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_ID" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>A user specified label for an address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine1" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE1" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 1 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine2" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE2" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 2 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine3" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE3" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 3 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine4" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE4" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 4 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine5" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE5" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 5 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine6" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE6" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 6 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AlternateEmailID" type="yfctype:EMailID" use="optional">
                <xsd:annotation yfc:columnName="ALTERNATE_EMAILID" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The alternate email address at which the person or a predesignated employee of the organization referred to in the address can be contacted.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Beeper" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="BEEPER" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The beeper number at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="City" type="yfctype:City" use="optional">
                <xsd:annotation yfc:columnName="CITY" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The city in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Company" type="yfctype:Company" use="optional">
                <xsd:annotation yfc:columnName="COMPANY" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The company this person is associated with.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Country" type="yfctype:Country" use="optional">
                <xsd:annotation yfc:columnName="COUNTRY" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The country in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DayFaxNo" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="DAY_FAX_NO" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The day time fax at which the person or a predesignated employee of the organization referred to in the address can be notified.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DayPhone" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="DAY_PHONE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The day time phone at which the person or organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Department" type="yfctype:Department" use="optional">
                <xsd:annotation yfc:columnName="DEPARTMENT" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The primary department in charge of procurement.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EMailID" type="yfctype:EMailID" use="optional">
                <xsd:annotation yfc:columnName="EMAILID" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The email address at which the person or a predesignated employee of the organization referred to in the address can be contacted.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EveningFaxNo" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="EVENING_FAX_NO" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The evening time fax at which the person or a predesignated employee of the organization referred to in the address can be notified.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EveningPhone" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="EVENING_PHONE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The evening time phone at which the person or organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FirstName" type="yfctype:PersonName" use="optional">
                <xsd:annotation yfc:columnName="FIRST_NAME" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The first name of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsAddressVerified" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_ADDRESS_VERIFIED" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>This flag indicates whether or not the address in this record has been verified by an address verification program. Intended for future use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsCommercialAddress" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_COMMERCIAL_ADDRESS" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>Indicates whether the address is commercial, residential or none. Values of this field can be Y -Commercial address.N - Residential address. undefined for Null.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="JobTitle" type="yfctype:Title-40" use="optional">
                <xsd:annotation yfc:columnName="JOB_TITLE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The job title of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LastName" type="yfctype:PersonName" use="optional">
                <xsd:annotation yfc:columnName="LAST_NAME" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The last name of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Latitude" use="optional">
                <xsd:annotation yfc:columnName="LATITUDE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The latitude of this location.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:DecimalWithNegative"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="Longitude" use="optional">
                <xsd:annotation yfc:columnName="LONGITUDE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The longitude of this location.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:DecimalWithNegative"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="MiddleName" type="yfctype:MiddleName" use="optional">
                <xsd:annotation yfc:columnName="MIDDLE_NAME" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The middle name of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MobilePhone" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="MOBILE_PHONE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The mobile phone at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OtherPhone" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="OTHER_PHONE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The alternate phone number at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PersonID" type="yfctype:PersonId" use="optional">
                <xsd:annotation yfc:columnName="PERSON_ID" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The unique identifier for a person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PersonInfoKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="PERSON_INFO_KEY" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The primary key for the YFS_PERSON_INFO table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="State" type="yfctype:State" use="optional">
                <xsd:annotation yfc:columnName="STATE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The state in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Suffix" type="yfctype:Suffix" use="optional">
                <xsd:annotation yfc:columnName="SUFFIX" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The suffix for a person, such as Jr., or Sr.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxGeoCode" type="yfctype:GeoCode" use="optional">
                <xsd:annotation yfc:columnName="TAX_GEO_CODE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The tax geo code for this location. Intended for future use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Title" type="yfctype:Title-10" use="optional">
                <xsd:annotation yfc:columnName="TITLE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The title for the person such as Ms., Mr., or Dr..</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ZipCode" type="yfctype:ZipCode" use="optional">
                <xsd:annotation yfc:columnName="ZIP_CODE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The zip code in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderTags">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrderTag"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderTag">
        <xsd:annotation yfc:tableName="YFS_ORDER_TAG">
            <xsd:documentation>This table stores tags on the order.  The tags can have different purposes and different usages.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="TagId" type="yfctype:Text-50" use="optional">
                <xsd:annotation yfc:columnName="TAG_ID" yfc:tableName="YFS_ORDER_TAG">
                    <xsd:documentation>
                        <div>
							Tag identifier.
							<Validation>
                                <Condition>
                                    <code>TagId</code> must be passed.
									</Condition>
                                <Error Description="TagId is missing in the input." ErrorCode="YFS85_0021"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TagType" type="yfctype:Text-50" use="optional">
                <xsd:annotation yfc:columnName="TAG_TYPE" yfc:tableName="YFS_ORDER_TAG">
                    <xsd:documentation>It indicates the purpose of a tag such as compatibility, client version which created the order, or it can contain any other configurable tag type.  If tag type is Y.COMPATABILITY, then corresponding TagId will be available for pipeline condition building as VersionTag attribute.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Opportunity">
        <xsd:annotation yfc:tableName="YFS_OPPORTUNITY">
            <xsd:documentation>
                <div>
					This tables stores an opportunity. An opportunity is a sales opportunity that a sales representative foresees and is used to keep track of multiple quotes that will be presented to a specific buyer. Any of these quotes can be negotiated upon but only one of these quotes can be accepted by the buyer, thereby permanently rejecting all other quotes. The opportunity will be closed once an order is created from the buyer selected quote.<br/>
                    <br/>
                    <code>EnterpriseCode</code>, <code>BuyerOrganizationCode</code>, <code>BillToID</code>,
					<code>CustomerContactID</code>, and <code>TeamCode</code> are defaulted from the quote, if not passed.
					<Validation>
                        <Condition>Input document is invalid.
						</Condition>
                        <Error Description="Input document is empty or contains invalid data." ErrorCode="YFS10363"/>
                    </Validation>
                    <Validation>
                        <Condition>Mandated parameters missing.
						</Condition>
                        <Error Description="Mandatory Parameters for the Operation are missing." ErrorCode="YFS10364"/>
                    </Validation>
                    <Validation>
                        <Condition>Opportunity record already existed.
						</Condition>
                        <Error Description="Record Already Exists" ErrorCode="YCP0051"/>
                    </Validation>
                    <Validation>
                        <Condition>Event template not found.
						</Condition>
                        <Error Description="Template xml for raising event not found" ErrorCode="YFS10429"/>
                    </Validation>
                </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:BuyerContactAddress"/>
                <xsd:element maxOccurs="1" minOccurs="0" name="Notes">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded" minOccurs="0" name="Note">
                                <xsd:annotation yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>Generic table used to stores notes associated with various entities in the system.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute name="AuditTransactionId" type="yfctype:TransactionID" use="optional">
                                    <xsd:annotation yfc:columnName="AUDIT_TRANSACTION_ID" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>The identifier of the order modification audit that caused this record.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ContactReference" type="yfctype:NotesContactReference" use="optional">
                                    <xsd:annotation yfc:columnName="CONTACT_REFERENCE" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>If the notes were entered because of customer contact by phone, the phone no can be stored in this column. If the contact is by email, email ID can be stored here.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ContactTime" type="yfctype:TimeStamp" use="optional">
                                    <xsd:annotation yfc:columnName="CONTACT_TIME" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>The time at which the customer contact occurred.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ContactType" type="yfctype:CommonCode" use="optional">
                                    <xsd:annotation yfc:columnName="CONTACT_TYPE" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>The contact type like Phone call, email etc. The values will be selected from common code of code type "CONTACT_TYPE"</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ContactUser" type="yfctype:UserId" use="optional">
                                    <xsd:annotation yfc:columnName="CONTACT_USER" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>The user who made the contact. The default value will the logged in user.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="CustomerSatIndicator" type="yfctype:CustomerSatisfactionLevel" use="optional">
                                    <xsd:annotation yfc:columnName="CUSTOMER_SAT_INDICATOR" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>The customer satisfaction level.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="NoteText" type="yfctype:NoteText" use="required">
                                    <xsd:annotation yfc:columnName="NOTE_TEXT" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>Detailed Notes.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Priority" type="yfctype:PriorityWithDecimal" use="optional">
                                    <xsd:annotation yfc:columnName="PRIORITY" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>Priority of the note. 1 is High, 0 is Low.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ReasonCode" type="yfctype:ReasonCode" use="optional">
                                    <xsd:annotation yfc:columnName="REASON_CODE" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>The reason for which the notes are logged. Like "CustomerContact" etc. The values will be selected from common code of code type "NOTES_REASON"</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="SequenceNo" type="yfctype:Count" use="optional">
                                    <xsd:annotation yfc:columnName="SEQUENCE_NO" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>The sequence of the note.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Tranid" type="yfctype:TranId" use="optional">
                                    <xsd:annotation yfc:columnName="TRANID" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>Transaction for which these notes are applicable.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="VisibleToAll" type="yfctype:Flag" use="optional">
                                    <xsd:annotation yfc:columnName="VISIBLE_TO_ALL" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>This flag determines whether the note applied should be visible to all users. This field is used for filtering. No logic is currently applied to it.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="CustomAttributes">
                    <xsd:annotation yfc:tableName="YFS_OPPORTUNITY_EXTENSION">
                        <xsd:documentation>This table stores dynamic extension attributes of opportunity entity.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType/>
                </xsd:element>
            </xsd:all>
            <xsd:attribute name="BillToID" type="yfctype:ID-40" use="optional">
                <xsd:annotation yfc:columnName="BILL_TO_ID" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>The identifier of the bill to customer. If a customer definition representing the buyer organization exists within the system, the BillToID can represent the CustomerID of the customer. Otherwise, the BillToID can represent the PersonID of the bill to address or the BuyerOrganizationCode of the buyer organization.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="BuyerContactAddressKey" type="yfctype:Text-24" use="optional">
                <xsd:annotation yfc:columnName="BUYER_CONTACT_ADDRESS_KEY" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>
                        <div>
			The buyer's contact address. For new records, the <code>BuyerContactAddressKey</code> is the bill to address key 
			defaulted from the customer contact address. If customer contact is not available, the customer account address is
			used, otherwise obtaining from the buyer organization default address.
			</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="BuyerOrganizationCode" type="yfctype:OrgCode" use="optional">
                <xsd:annotation yfc:columnName="BUYER_ORGANIZATION_CODE" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>
                        <div>
			The buyer organization that is associated with this opportunity. This must be a valid organization defined in the system.
			<Validation>
                                <Condition>
                                    <code>BuyerOrganizationCode</code> must be valid in the configuration
				</Condition>
                                <Error Description="Invalid Buyer Organization" ErrorCode="YFS10368"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CoOwnerUserID" type="yfctype:ID-40" use="optional">
                <xsd:annotation yfc:columnName="CO_OWNER_USER_ID" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>
                        <div>
			The co-owner user is a user who jointly owns this opportunity with the orginal owner user.
			<Validation>
                                <Condition>Invalid User identifiers passed.
				</Condition>
                                <Error Description="Invalid User identifiers passed." ErrorCode="YCP0084"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CurrencyValue" use="optional">
                <xsd:annotation yfc:columnName="CURRENCY_VALUE" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>The currency value or budget of the opportunity.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Amount"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="CustomerContactID" type="yfctype:ID-40" use="optional">
                <xsd:annotation yfc:columnName="CUSTOMER_CONTACT_ID" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>
                        <div>
			This is the customer contact ID for the opportunity. This field maps to contact ID field of YFS_Customer_Contact.
			<Validation>
                                <Condition>Invalid Contact Identifier.
				</Condition>
                                <Error Description="YFS: Invalid Customer Id" ErrorCode="YFS10319"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Description" type="yfctype:Text-200" use="optional">
                <xsd:annotation yfc:columnName="DESCRIPTION" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>The detailed description of the opportunity.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DocumentType" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="DOCUMENT_TYPE" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>
                        <div>
			The document type that is associated with this opportunity. For example, Opportunity document (0020). OpportunityID, DocumentType, EnterpriseCode are required attributes to uniquely identify an opportunity.
			<Validation>
                                <Condition>
                                    <code>DocumentType</code> must be defined in the configuration
				</Condition>
                                <Error Description="Invalid Document Type" ErrorCode="YFS10367"/>
                            </Validation>
                            <Validation>
                                <Condition>Opportunity record does not exist.
				</Condition>
                                <Error Description="YCP:No Record Found." ErrorCode="YCP0045"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EnterpriseCode" type="yfctype:OrgCode" use="required">
                <xsd:annotation yfc:columnName="ENTERPRISE_CODE" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>
                        <div>
			The enterprise code. This field is a part of an opportunity's unique key. The EnterpriseCode, DocumentType and OpportunityID together uniquely identify an opportunity. <br/>Default value: <code>DEFAULT</code>
                            <Validation>
                                <Condition>
                                    <code>EnterpriseCode</code> must be valid in the configuration
				</Condition>
                                <Error Description="YFS:Invalid Enterprise Code" ErrorCode="YFS10113"/>
                            </Validation>
                            <Validation>
                                <Condition>Opportunity record does not exist.
				</Condition>
                                <Error Description="YCP:No Record Found." ErrorCode="YCP0045"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ExpectedCloseDate" use="optional">
                <xsd:annotation yfc:columnName="EXPECTED_CLOSE_DATE" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>The anticipated close date for the opportunity.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:TimeStamp"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="LeadOrigin" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="LEAD_ORIGIN" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>The original lead to creation of this opportunity. E.g. Trade Shows, Call Center, Existing Customer.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LostReasonCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="LOST_REASON_CODE" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>If the opportunity is lost, this specifies the reason for losing this opportunity.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OpportunityDate" use="optional">
                <xsd:annotation yfc:columnName="OPPORTUNITY_DATE" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>The date that the opportunity was created.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:TimeStamp"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="OpportunityID" type="yfctype:ID-40" use="optional">
                <xsd:annotation yfc:columnName="OPPORTUNITY_ID" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>
                        <div>
			The identification number of the opportunity, which is required during opportunity creation. If not passed the <code>OpportunityID</code> generates based on a system rule setting or from the user exit implementation. Whenever modification is required, <code>OpportunityID</code> is mandatory. OpportunityID, DocumentType, EnterpriseCode are required attributes to uniquely identify an opportunity.
			<Validation>
                                <Condition>Mandatory parameter missing.
				</Condition>
                                <Error Description="YCP:No Record Found." ErrorCode="YCP0045"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OpportunityName" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="OPPORTUNITY_NAME" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>A custom name given to this opportunity to help identify the record uniquely.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OwnerUserID" type="yfctype:ID-40" use="optional">
                <xsd:annotation yfc:columnName="OWNER_USER_ID" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>
                        <div>
			The user in which this opportunity is assigned to.
			<Validation>
                                <Condition>Invalid User identifiers passed.
				</Condition>
                                <Error Description="Invalid User identifiers passed." ErrorCode="YCP0084"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PipelineKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="PIPELINE_KEY" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>Pipeline this opportunity belongs to. The pipeline key is determined based on the input attribute passed and the pipeline conditions in the system configuration.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ProbableSuccessRate" use="optional">
                <xsd:annotation yfc:columnName="PROBABLE_SUCCESS_RATE" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>The probability of success of the opportunity.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Percentage"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="TeamCode" type="yfctype:CodeLongDescription" use="optional">
                <xsd:annotation yfc:columnName="TEAM_CODE" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>Indicates a team that can manage this opportunity.  Default is Null.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="BuyerContactAddress">
        <xsd:annotation yfc:tableName="YFS_PERSON_INFO">
            <xsd:documentation>This table stores relevant personal information and addresses. Addresses are typically organization address, ship node address, customer address, and so forth. Personal information is required in cases where person to person contact needs to be established; for example, taking appointments or speaking during negotiations. The key of this table is posted in various tables to allow access to people and address information.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="AddressID" type="yfctype:Text-256" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_ID" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>A user specified label for an address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine1" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE1" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 1 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine2" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE2" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 2 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine3" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE3" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 3 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine4" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE4" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 4 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine5" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE5" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 5 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine6" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE6" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 6 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AlternateEmailID" type="yfctype:EMailID" use="optional">
                <xsd:annotation yfc:columnName="ALTERNATE_EMAILID" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The alternate email address at which the person or a predesignated employee of the organization referred to in the address can be contacted.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Beeper" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="BEEPER" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The beeper number at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="City" type="yfctype:City" use="optional">
                <xsd:annotation yfc:columnName="CITY" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The city in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Company" type="yfctype:Company" use="optional">
                <xsd:annotation yfc:columnName="COMPANY" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The company this person is associated with.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Country" type="yfctype:Country" use="optional">
                <xsd:annotation yfc:columnName="COUNTRY" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The country in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DayFaxNo" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="DAY_FAX_NO" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The day time fax at which the person or a predesignated employee of the organization referred to in the address can be notified.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DayPhone" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="DAY_PHONE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The day time phone at which the person or organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Department" type="yfctype:Department" use="optional">
                <xsd:annotation yfc:columnName="DEPARTMENT" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The primary department in charge of procurement.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EMailID" type="yfctype:EMailID" use="optional">
                <xsd:annotation yfc:columnName="EMAILID" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The email address at which the person or a predesignated employee of the organization referred to in the address can be contacted.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EveningFaxNo" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="EVENING_FAX_NO" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The evening time fax at which the person or a predesignated employee of the organization referred to in the address can be notified.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EveningPhone" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="EVENING_PHONE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The evening time phone at which the person or organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FirstName" type="yfctype:PersonName" use="optional">
                <xsd:annotation yfc:columnName="FIRST_NAME" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The first name of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsAddressVerified" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_ADDRESS_VERIFIED" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>This flag indicates whether or not the address in this record has been verified by an address verification program. Intended for future use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsCommercialAddress" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_COMMERCIAL_ADDRESS" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>Indicates whether the address is commercial, residential or none. Values of this field can be Y -Commercial address.N - Residential address. undefined for Null.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="JobTitle" type="yfctype:Title-40" use="optional">
                <xsd:annotation yfc:columnName="JOB_TITLE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The job title of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LastName" type="yfctype:PersonName" use="optional">
                <xsd:annotation yfc:columnName="LAST_NAME" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The last name of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Latitude" use="optional">
                <xsd:annotation yfc:columnName="LATITUDE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The latitude of this location.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:DecimalWithNegative"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="Longitude" use="optional">
                <xsd:annotation yfc:columnName="LONGITUDE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The longitude of this location.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:DecimalWithNegative"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="MiddleName" type="yfctype:MiddleName" use="optional">
                <xsd:annotation yfc:columnName="MIDDLE_NAME" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The middle name of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MobilePhone" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="MOBILE_PHONE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The mobile phone at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OtherPhone" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="OTHER_PHONE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The alternate phone number at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PersonID" type="yfctype:PersonId" use="optional">
                <xsd:annotation yfc:columnName="PERSON_ID" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The unique identifier for a person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PersonInfoKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="PERSON_INFO_KEY" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The primary key for the YFS_PERSON_INFO table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="State" type="yfctype:State" use="optional">
                <xsd:annotation yfc:columnName="STATE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The state in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Suffix" type="yfctype:Suffix" use="optional">
                <xsd:annotation yfc:columnName="SUFFIX" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The suffix for a person, such as Jr., or Sr.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxGeoCode" type="yfctype:GeoCode" use="optional">
                <xsd:annotation yfc:columnName="TAX_GEO_CODE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The tax geo code for this location. Intended for future use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Title" type="yfctype:Title-10" use="optional">
                <xsd:annotation yfc:columnName="TITLE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The title for the person such as Ms., Mr., or Dr..</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ZipCode" type="yfctype:ZipCode" use="optional">
                <xsd:annotation yfc:columnName="ZIP_CODE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The zip code in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="FileAttachments">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:FileAttachment"/>
            </xsd:all>
            <xsd:attribute name="TotalNumberOfRecords" type="yfctype:Number" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Number of Attachments in the Order.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="FileAttachment">
        <xsd:annotation yfc:tableName="PLT_FILE_ATTACHMENT">
            <xsd:documentation>This table stores information about uploaded files, documents.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="FileAttachmentKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="FILE_ATTACHMENT_KEY" yfc:tableName="PLT_FILE_ATTACHMENT">
                    <xsd:documentation>The key of the attached file</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FileContentType" type="yfctype:FileContentType" use="optional">
                <xsd:annotation yfc:columnName="FILE_CONTENT_TYPE" yfc:tableName="PLT_FILE_ATTACHMENT">
                    <xsd:documentation>Type of the file content as returned by FileUploadServlet</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FileDescription" type="yfctype:Description-255" use="optional">
                <xsd:annotation yfc:columnName="FILE_DESCRIPTION" yfc:tableName="PLT_FILE_ATTACHMENT">
                    <xsd:documentation>Description of the file content as returned by FileUploadServlet</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FileLocation" type="yfctype:FileLocation" use="optional">
                <xsd:annotation>
                    <xsd:documentation>The location of the file as returned by FileUploadServlet</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FileName" type="yfctype:FileName" use="optional">
                <xsd:annotation yfc:columnName="FILE_NAME" yfc:tableName="PLT_FILE_ATTACHMENT">
                    <xsd:documentation>Complete name of the file as returned by FileUploadServlet</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FileSize" use="optional">
                <xsd:annotation yfc:columnName="FILE_SIZE" yfc:tableName="PLT_FILE_ATTACHMENT">
                    <xsd:documentation>Size of the file  as returned by FileUploadServlet</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:FileSize"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="Operation" type="yfctype:String" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>Action to take
							<br/>Valid Value: 
							<br/>
                            <b>
                                <code>Delete</code>
                            </b> - Deletes the file attachment.
						</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>