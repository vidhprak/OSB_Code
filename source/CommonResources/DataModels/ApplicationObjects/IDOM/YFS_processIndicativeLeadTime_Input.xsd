				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="ItemAttributeGroupKey" type="yfctype:Key" use="optional">
				<xsd:annotation yfc:columnName="ITEM_ATTR_GROUP_KEY" yfc:tableName="YFS_ITEM_ATTR_GROUP">
