				</xsd:annotation>
				<xsd:simpleType>
					<xsd:union memberTypes="yfctype:Count">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value=""/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:union>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ItemAttributeList">
		<xsd:complexType>
