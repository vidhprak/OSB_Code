				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="ItemAttributeGroupPath" type="yfctype:Text-800" use="optional">
				<xsd:annotation yfc:columnName="ITEM_ATTRIBUTE_GROUP_PATH" yfc:tableName="YFS_ITEM_ATTR_GROUP">
					<xsd:documentation>The path of the attribute group within the attribute domain.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="ParentItemAttributeGroupKey" type="yfctype:Key" use="optional">
				<xsd:annotation yfc:columnName="PARENT_ITEM_ATTR_GRP_KEY" yfc:tableName="YFS_ITEM_ATTR_GROUP">
